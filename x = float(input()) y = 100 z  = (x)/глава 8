print("\n=== 8.29. Числа с k делителями от a до b ===")
print("8.29 (пример: a=10, b=30, k=4):")
a, b, k = 10, 30, 4
for num in range(a, b + 1):
    count = 0
    for d in range(1, num + 1):
        if num % d == 0:
            count += 1
    if count == k:
        print(num, end=' ')
print()

print("\n=== 8.30. Число с максимальным количеством делителей ===")
print("8.30 (пример: a=1, b=50):")
a, b = 1, 50
max_count = 0
max_num = a
for num in range(a, b + 1):
    count = 0
    for d in range(1, num + 1):
        if num % d == 0:
            count += 1
    if count > max_count:
        max_count = count
        max_num = num
print(f"Число с максимальным количеством делителей: {max_num}, делителей: {max_count}")

print("\n=== 8.31. Трехзначные простые числа ===")
print("8.31:")
for num in range(100, 1000):
    is_prime = True
    for d in range(2, int(num**0.5) + 1):
        if num % d == 0:
            is_prime = False
            break
    if is_prime:
        print(num, end=' ')
print()

print("\n=== 8.32. Первые 100 простых чисел ===")
print("8.32 (первые 20 для примера):")
count = 0
num = 2
while count < 20:
    is_prime = True
    for d in range(2, int(num**0.5) + 1):
        if num % d == 0:
            is_prime = False
            break
    if is_prime:
        print(num, end=' ')
        count += 1
    num += 1
print("...")

