
print("=== 8.1. Организация вывода ===")
# 8.1 а
print("8.1 а:")
for i in range(4):
    for j in range(6):
        print(5, end=' ')
    print()

# 8.1 б
print("\n8.1 б:")
for i in range(4):
    for j in range(1, 11):
        print(j, end=' ')
    print()

# 8.1 в
print("\n8.1 в:")
for i in range(41, 81, 10):
    for j in range(i, i + 10):
        print(j, end=' ')
    print()

print("\n=== 8.2. Таблицы с уменьшением ===")
# 8.2 а
print("8.2 а:")
for i in range(1, 6):
    for j in range(i):
        print(5, end=' ')
    print()

# 8.2 б
print("\n8.2 б:")
for i in range(5, 0, -1):
    for j in range(i):
        print(1, end=' ')
    print()

print("\n=== 8.3. Таблицы с числами ===")
# 8.3 а
print("8.3 а:")
for i in range(1, 6):
    for j in range(i):
        print(i, end=' ')
    print()

# 8.3 б
print("\n8.3 б:")
k = 5
for i in range(5, 0, -1):
    for j in range(i):
        print(k, end=' ')
    k += 1
    print()

# 8.3 в
print("\n8.3 в:")
k = 10
for i in range(1, 6):
    for j in range(i):
        print(k, end=' ')
    k += 10
    print()

# 8.3 г
print("\n8.3 г:")
k = 5
for i in range(5, 0, -1):
    for j in range(i):
        print(k, end=' ')
    k += 5
    print()

print("\n=== 8.4. Таблицы со сложной нумерацией ===")
# 8.4 а
print("8.4 а:")
for i in range(5):
    for j in range(5 - i):
        print(j + i, end=' ')
    print()

# 8.4 б
print("\n8.4 б:")
for i in range(5):
    for j in range(5):
        print(30 - (i + j), end=' ')
    print()

print("\n=== 8.5-8.8. Таблицы сложения и умножения ===")
# 8.5
print("8.5 (сложение горизонтально):")
for i in range(1, 10):
    for j in range(1, 10):
        print(f"{i}+{j}={i+j}", end=' ')
    print()

# 8.6
print("\n8.6 (сложение вертикально):")
for i in range(1, 10):
    for j in range(1, 10):
        print(f"{j}+{i}={j+i}", end=' ')
    print()

# 8.7
print("\n8.7 (умножение горизонтально):")
for i in range(1, 10):
    for j in range(1, 10):
        print(f"{i}x{j}={i*j}", end=' ')
    print()

# 8.8
print("\n8.8 (умножение вертикально):")
for i in range(1, 10):
    for j in range(1, 10):
        print(f"{j}x{i}={j*i}", end=' ')
    print()

print("\n=== 8.9. Ввод/вывод таблицы оценок ===")
print("8.9 (для экономии места ввод имитирован):")
table = [[4,5,3],[3,4,4],[5,5,5],[4,3,4],[5,4,5],[3,4,3],[4,5,4],[5,5,5],[4,4,4],[3,3,3],[5,4,5],[4,5,4],[5,5,4],[4,4,5],[3,4,3],[5,4,4],[4,3,5],[3,5,4]]
print("Вывод таблицы 18×3:")
for row in table:
    print(row)

print("\n=== 8.10. Ввод/вывод таблицы баллов ===")
print("8.10 (ввод имитирован):")
table = [[6,7,8,9,7],[5,6,7,8,6],[7,8,9,7,8],[6,7,8,9,7],[8,9,7,8,9],[7,8,9,7,8],[9,7,8,9,7],[8,9,7,8,9]]
print("Вывод таблицы 8×5:")
for row in table:
    print(row)

print("\n=== 8.11. Зарплата работников ===")
print("8.11 (ввод имитирован):")
workers = 3  # для примера 3 вместо 12
months = 3
table = [[30000,32000,31000],[28000,29000,30000],[35000,34000,36000]]

total = 0
for i in range(workers):
    for j in range(months):
        total += table[i][j]
print(f"Общая сумма за квартал: {total}")

print("Зарплата каждого работника:")
for i in range(workers):
    s = sum(table[i])
    print(f"Работник {i+1}: {s}")

print("Общая зарплата за каждый месяц:")
for j in range(months):
    s = sum(table[i][j] for i in range(workers))
    print(f"Месяц {j+1}: {s}")

print("\n=== 8.12. Результаты фигуристов ===")
print("8.12 (ввод имитирован):")
sportsmen = 3  # для примера 3 вместо 15
programs = 3
table = [[8.5,9.0,8.7],[9.1,8.8,9.2],[8.9,9.1,8.8]]

print("Средний балл каждого спортсмена:")
for i in range(sportsmen):
    avg = sum(table[i]) / programs
    print(f"Спортсмен {i+1}: {avg:.2f}")

print("Средний балл по каждой программе:")
for j in range(programs):
    s = sum(table[i][j] for i in range(sportsmen))
    avg = s / sportsmen
    print(f"Программа {j+1}: {avg:.2f}")

print("\n=== 8.13. Оценки учеников ===")
print("8.13 (ввод имитирован):")
students = 3  # для примера 3 вместо 18
subjects = 3
table = [[5,4,5],[3,4,3],[4,5,4]]

fives = 0
for i in range(students):
    for j in range(subjects):
        if table[i][j] == 5:
            fives += 1
print(f"Пятёрок всего: {fives}")

print("Количество троек у каждого ученика:")
for i in range(students):
    threes = sum(1 for x in table[i] if x == 3)
    print(f"Ученик {i+1}: троек = {threes}")

print("Количество двоек по каждому предмету:")
for j in range(subjects):
    twos = sum(1 for i in range(students) if table[i][j] == 2)
    print(f"Предмет {j+1}: двоек = {twos}")

print("\n=== 8.14. Оценки студентов в сессию ===")
print("8.14 (ввод имитирован):")
students = 3  # для примера 3 вместо 15
subjects = 3
table = [[4,5,4],[3,4,3],[5,5,5]]

good_students = sum(1 for i in range(students) if 2 not in table[i])
print(f"Студентов без двоек: {good_students}")

perfect_subjects = 0
for j in range(subjects):
    marks = [table[i][j] for i in range(students)]
    if all(m in (4,5) for m in marks):
        perfect_subjects += 1
print(f"Предметов без троек и двоек: {perfect_subjects}")

print("Двойки по каждому предмету:")
for j in range(subjects):
    twos = sum(1 for i in range(students) if table[i][j] == 2)
    print(f"Предмет {j+1}: двоек = {twos}")

print("\n=== 8.15. Баллы пятиборцев ===")
print("8.15 (ввод имитирован):")
sportsmen = 3  # для примера 3 вместо 8
sports = 5
table = [[7,8,9,8,7],[8,9,8,9,8],[9,8,9,8,9]]

max_ball = max(max(row) for row in table)
print(f"Максимальный балл: {max_ball}")

max_sum = 0
winner = 0
for i in range(sportsmen):
    s = sum(table[i])
    if s > max_sum:
        max_sum = s
        winner = i + 1
print(f"Победитель: спортсмен {winner}, баллов: {max_sum}")

print("\n=== 8.16. Максимальная зарплата ===")
print("8.16 (ввод имитирован):")
workers = 3  # для примера 3 вместо 12
months = 3
table = [[30000,32000,31000],[28000,29000,30000],[35000,34000,36000]]

max_salary = max(max(row) for row in table)
print(f"Максимальная зарплата: {max_salary}")

best_worker = 0
best_sum = 0
for i in range(workers):
    s = sum(table[i])
    if s > best_sum:
        best_sum = s
        best_worker = i + 1
print(f"Лучший работник: {best_worker}, сумма: {best_sum}")

best_month = 0
best_month_sum = 0
for j in range(months):
    s = sum(table[i][j] for i in range(workers))
    if s > best_month_sum:
        best_month_sum = s
        best_month = j + 1
print(f"Лучший месяц: {best_month}, сумма: {best_month_sum}")

print("\n=== 8.17. Лучшая зарплата по месяцам и работникам ===")
print("8.17 (ввод имитирован, те же данные):")
print("Для каждого работника — лучший месяц:")
for i in range(workers):
    max_s = max(table[i])
    month = table[i].index(max_s) + 1
    print(f"Работник {i+1}: лучший месяц {month}")

print("Для каждого месяца — лучший работник:")
for j in range(months):
    max_s = max(table[i][j] for i in range(workers))
    for i in range(workers):
        if table[i][j] == max_s:
            print(f"Месяц {j+1}: лучший работник {i+1}")
            break

print("\n=== 8.18. Ученики по классам и параллелям ===")
print("8.18 (ввод имитирован):")
parallels = 3  # для примера 3 вместо 11
classes = 4
table = [[25,27,22,24],[24,26,23,25],[20,25,21,26]]

min_class = min(min(row) for row in table)
print(f"Самый малочисленный класс: {min_class}")

min_parallel = min(sum(row) for row in table)
print(f"Минимальная сумма по параллели: {min_parallel}")

print("Сумма по классам А, Б, В, Г:")
for j in range(classes):
    s = sum(table[i][j] for i in range(parallels))
    print(f"Класс {j+1}: {s}")

print("\n=== 8.19. Самый малочисленный класс в каждой параллели ===")
print("8.19 (те же данные):")
print("В каждой параллели — минимальный класс:")
for i in range(parallels):
    min_in_parallel = min(table[i])
    print(f"Параллель {i+1}: {min_in_parallel}")

print("Для каждого буквенного класса — минимальное значение:")
for j in range(classes):
    min_in_class = min(table[i][j] for i in range(parallels))
    print(f"Класс {j+1}: {min_in_class}")

print("\n=== 8.20. Доходы магазинов ===")
print("8.20 (ввод имитирован):")
shops = 3
days = 5  # для примера 5 вместо 10
table = [[1000,1200,1100,1300,1000],[900,1100,1000,1200,1100],[1300,1400,1200,1100,1300]]

max_shop = 0
max_sum = 0
for i in range(shops):
    s = sum(table[i])
    if s > max_sum:
        max_sum = s
        max_shop = i + 1
print(f"Лучший магазин: {max_shop}, доход: {max_sum}")

max_day = 0
max_day_sum = 0
for j in range(days):
    s = sum(table[i][j] for i in range(shops))
    if s > max_day_sum:
        max_day_sum = s
        max_day = j + 1
print(f"Лучший день: {max_day}, доход: {max_day_sum}")

max_income = 0
max_shop_day = 0
max_day_num = 0
for i in range(shops):
    for j in range(days):
        if table[i][j] > max_income:
            max_income = table[i][j]
            max_shop_day = i + 1
            max_day_num = j + 1
print(f"Максимальный доход за день: магазин {max_shop_day}, день {max_day_num}, сумма {max_income}")

print("\n=== 8.21. Лучший день для каждого магазина ===")
print("8.21 (те же данные):")
print("Для каждого магазина — лучший день:")
for i in range(shops):
    max_d = max(table[i])
    day = table[i].index(max_d) + 1
    print(f"Магазин {i+1}: день {day}")

print("Для каждого дня — лучший магазин:")
for j in range(days):
    max_inc = max(table[i][j] for i in range(shops))
    for i in range(shops):
        if table[i][j] == max_inc:
            print(f"День {j+1}: магазин {i+1}")
            break

print("\n=== 8.22. Студенты по курсам и группам ===")
print("8.22 (ввод имитирован):")
courses = 3  # для примера 3 вместо 5
groups = 4   # для примера 4 вместо 6
table = [[20,22,21,19],[18,20,19,21],[22,21,20,23]]

min_course = 0
min_total = float('inf')
for i in range(courses):
    s = sum(table[i])
    if s < min_total:
        min_total = s
        min_course = i + 1
print(f"Самый малочисленный курс: {min_course}, студентов: {min_total}")

min_group_val = float('inf')
min_course_gr = 0
min_group_gr = 0
for i in range(courses):
    for j in range(groups):
        if table[i][j] < min_group_val:
            min_group_val = table[i][j]
            min_course_gr = i + 1
            min_group_gr = j + 1
print(f"Самая малочисленная группа: курс {min_course_gr}, группа {min_group_gr}, студентов: {min_group_val}")

print("Самая маленькая группа на каждом курсе:")
for i in range(courses):
    min_in_course = min(table[i])
    group = table[i].index(min_in_course) + 1
    print(f"Курс {i+1}: группа {group} ({min_in_course} чел.)")

print("\n=== 8.23. Продажи товаров по дням ===")
print("8.23 (ввод имитирован):")
types = 3  # для примера 3 вместо 5
days = 4   # для примера 4 вместо 6
prices = [100, 200, 150]
table = [[5,6,4,7],[3,4,5,4],[6,5,7,6]]

print("Общий доход по каждому виду товара:")
for i in range(types):
    total = sum(table[i][j] * prices[i] for j in range(days))
    print(f"Товар {i+1}: {total}")

print("Общий доход за каждый день:")
for j in range(days):
    total = sum(table[i][j] * prices[i] for i in range(types))
    print(f"День {j+1}: {total}")

total_income = sum(table[i][j] * prices[i] for i in range(types) for j in range(days))
print(f"Общий доход за {days} дней: {total_income}")

max_income = 0
best_product = 0
for i in range(types):
    income = sum(table[i][j] * prices[i] for j in range(days))
    if income > max_income:
        max_income = income
        best_product = i + 1
print(f"Товар с максимальным доходом: {best_product}, доход: {max_income}")

max_day_income = 0
best_day = 0
for j in range(days):
    income = sum(table[i][j] * prices[i] for i in range(types))
    if income > max_day_income:
        max_day_income = income
        best_day = j + 1
print(f"День с максимальным доходом: {best_day}, доход: {max_day_income}")

a = 2000
days_above_a = sum(1 for j in range(days) if sum(table[i][j] * prices[i] for i in range(types)) > a)
print(f"Дней с доходом больше {a} рублей: {days_above_a}")

print("\n=== 8.24. Лучшая группа по среднему баллу ===")
print("8.24 (ввод имитирован):")
groups = 2  # для примера 2 вместо 3
students = 3  # для примера 3 вместо 20
exams = 3
tables = [
    [[4,5,4],[5,4,5],[4,4,5]],
    [[5,5,5],[4,5,4],[5,4,5]]
]

best_group = 0
best_avg = 0
for g in range(groups):
    total = 0
    for i in range(students):
        for j in range(exams):
            total += tables[g][i][j]
    avg = total / (students * exams)
    if avg > best_avg:
        best_avg = avg
        best_group = g + 1
print(f"Лучшая группа: {best_group}, средний балл: {best_avg:.2f}")

print("\n=== 8.25. Количество делителей чисел от 120 до 140 ===")
print("8.25:")
for num in range(120, 141):
    count = 0
    for d in range(1, num + 1):
        if num % d == 0:
            count += 1
    print(f"{num}: {count} делителей", end=' | ' if (num-119) % 4 != 0 else '\n')

print("\n=== 8.26. Графическое изображение делимости ===")
print("8.26 (для n=10):")
n = 10
for num in range(1, n + 1):
    count = 0
    for d in range(1, num + 1):
        if num % d == 0:
            count += 1
    print(f"{num} " + "+" * count)

print("\n=== 8.27. Числа с ровно 5 делителями (1-300) ===")
print("8.27:")
for num in range(1, 301):
    count = 0
    for d in range(1, num + 1):
        if num % d == 0:
            count += 1
    if count == 5:
        print(num, end=' ')
print()

