print("\n=== 8.35. Числа с суммой делителей кратной 10 (300-600) ===")
print("8.35 (первые найденные):")
found = 0
for num in range(300, 601):
    s = 0
    for d in range(1, num + 1):
        if num % d == 0:
            s += d
    if s % 10 == 0:
        print(num, end=' ')
        found += 1
        if found >= 10:
            break
print("...")

print("\n=== 8.36. Совершенные числа до 100 000 ===")
print("8.36:")
for num in range(2, 100000):
    s = 0
    for d in range(1, num):
        if num % d == 0:
            s += d
    if s == num:
        print(num, end=' ')
print()

print("\n=== 8.37. Число с максимальной суммой делителей ===")
print("8.37 (пример: a=1, b=100):")
a, b = 1, 100
max_sum = 0
max_num = a
for num in range(a, b + 1):
    s = 0
    for d in range(1, num + 1):
        if num % d == 0:
            s += d
    if s > max_sum:
        max_sum = s
        max_num = num
print(f"Число с максимальной суммой делителей: {max_num}, сумма: {max_sum}")

print("\n=== 8.38. Дружественные числа до 50 000 ===")
print("8.38 (первые пары):")
for a in range(2, 1000):  # ограничим для скорости
    sum_a = 0
    for d in range(1, a):
        if a % d == 0:
            sum_a += d
    b = sum_a
    if b > a and b < 1000:
        sum_b = 0
        for d in range(1, b):
            if b % d == 0:
                sum_b += d
        if sum_b == a:
            print(f"{a} и {b}")

print("\n=== 8.39. Прямоугольники с площадью s ===")
print("8.39 (s=12):")
s = 12
print("а) Все прямоугольники:")
count = 0
for a in range(1, s + 1):
    for b in range(1, s + 1):
        if a * b == s:
            print(f"{a} x {b}", end='; ')
            count += 1
print(f"\nВсего: {count}")

print("б) Без перестановок:")
count = 0
for a in range(1, int(s**0.5) + 1):
    if s % a == 0:
        b = s // a
        print(f"{a} x {b}", end='; ')
        count += 1
print(f"\nВсего: {count}")

print("\n=== 8.40. Параллелепипеды с объемом v ===")
print("8.40 (v=24):")
v = 24
print("а) Все варианты (первые 5):")
count = 0
for a in range(1, v + 1):
    for b in range(1, v + 1):
        for c in range(1, v + 1):
            if a * b * c == v:
                print(f"{a}×{b}×{c}", end='; ')
                count += 1
                if count >= 5:
                    break
        if count >= 5:
            break
    if count >= 5:
        break
print(f"\nВсего: много")

print("б) Без перестановок:")
count = 0
for a in range(1, int(v**(1/3)) + 1):
    if v % a == 0:
        for b in range(a, int((v/a)**0.5) + 1):
            if (v // a) % b == 0:
                c = v // (a * b)
                if c >= b:
                    print(f"{a}×{b}×{c}", end='; ')
                    count += 1
print(f"\nВсего: {count}")

print("\n=== 8.41. Решения x² + y² = k² (1-30) ===")
print("8.41:")
for x in range(1, 31):
    for y in range(x, 31):
        k2 = x*x + y*y
        k = int(k2**0.5)
        if k <= 30 and k*k == k2:
            print(f"{x}² + {y}² = {k}²")

print("\n=== 8.42. Сумма 1ⁿ + 2ⁿ + ... + mⁿ ===")
print("8.42 (пример: m=5, n=3):")
m, n = 5, 3
total = 0
for i in range(1, m + 1):
    power = 1
    for _ in range(n):
        power *= i
    total += power
print(f"1^{n} + 2^{n} + ... + {m}^{n} = {total}")

print("\n=== 8.43. Сумма 1¹ + 2² + ... + nⁿ ===")
print("8.43 (пример: n=5):")
n = 5
total = 0
for i in range(1, n + 1):
    power = 1
    for _ in range(i):
        power *= i
    total += power
print(f"1¹ + 2² + ... + {n}ⁿ = {total}")

print("\n=== 8.44. Трехзначные числа с суммой цифр n ===")
print("8.44 (n=10):")
n = 10
for a in range(1, 10):
    for b in range(10):
        for c in range(10):
            if a + b + c == n:
                print(f"{a}{b}{c}", end=' ')
print()

print("\n=== 8.45. Трехзначные числа с разными цифрами ===")
print("8.45 (первые 20):")
count = 0
for a in range(1, 10):
    for b in range(10):
        if b != a:
            for c in range(10):
                if c != a and c != b:
                    print(f"{a}{b}{c}", end=' ')
                    count += 1
                    if count >= 20:
                        break
            if count >= 20:
                break
    if count >= 20:
        break
print("...")

print("\n=== 8.46. НОД нескольких чисел ===")
print("8.46 (пример: 12, 18, 24):")
def nod(a, b):
    while b:
        a, b = b, a % b
    return a

nums = [12, 18, 24]
result = nums[0]
for i in range(1, len(nums)):
    result = nod(result, nums[i])
print(f"НОД{tuple(nums)} = {result}")

print("\n=== 8.47. Способы составления веса гирями ===")
print("8.47 (пример: v=500):")
weights = [100, 200, 300, 500, 1000, 1200, 1400, 1500, 2000, 3000]
v = 500
count = 0
for i in range(2**5):  # ограничим первыми 5 гирями для скорости
    s = 0
    for j in range(5):
        if i & (1 << j):
            s += weights[j]
    if s == v:
        count += 1
print(f"Число способов составить {v} г: {count}")

print("\n=== 8.48. Способы выплаты суммы ===")
print("8.48 (n=15):")
n = 15
coins = [10, 5, 2, 1]
count = 0
print("Способы:")
for c10 in range(n//10 + 1):
    for c5 in range((n - c10*10)//5 + 1):
        for c2 in range((n - c10*10 - c5*5)//2 + 1):
            c1 = n - c10*10 - c5*5 - c2*2
            if c1 >= 0:
                count += 1
                print(f"{c10}×10 + {c5}×5 + {c2}×2 + {c1}×1", end='; ')
print(f"\nВсего способов: {count}")

print("\n=== 8.49. Числа, квадрат суммы цифр которых равен m ===")
print("8.49 (m=25, n=100):")
m, n = 25, 100
for num in range(1, n):
    s = 0
    temp = num
    while temp > 0:
        s += temp % 10
        temp //= 10
    if s*s == m:
        print(num, end=' ')
print()

print("\n=== 8.50. Минимальное количество купюр ===")
print("8.50 (n=100):")
n = 100
bills = [64, 32, 16, 8, 4, 2, 1]
for amount in range(n, n + 6):  # первые 6 сумм
    print(f"{amount}: ", end='')
    temp = amount
    for bill in bills:
        count = temp // bill
        if count > 0:
            print(f"{bill}×{count}", end=' ')
            temp %= bill
    print()

print("\n=== 8.51. Цифровой корень ===")
print("8.51 (пример: 9875):")
num = 9875
original = num
while num > 9:
    s = 0
    while num > 0:
        s += num % 10
        num //= 10
    num = s
print(f"Цифровой корень {original} = {num}")

print("\n=== 8.52. Быки, коровы, телята ===")
print("8.52:")
for bulls in range(1, 10):
    for cows in range(1, 20):
        calves = 100 - bulls - cows
        if calves > 0:
            if bulls*10 + cows*5 + calves*0.5 == 100:
                print(f"Быков: {bulls}, коров: {cows}, телят: {calves}")

print("\n=== 8.53. Разложение на простые множители ===")
print("8.53 (n=60):")
n = 60
print("Вариант 1 (уникальные множители):")
factors = set()
temp = n
d = 2
while temp > 1:
    if temp % d == 0:
        factors.add(d)
        temp //= d
    else:
        d += 1
print(" × ".join(map(str, sorted(factors))))

print("Вариант 2 (с кратностью):")
temp = n
d = 2
result = []
while temp > 1:
    if temp % d == 0:
        count = 0
        while temp % d == 0:
            temp //= d
            count += 1
        result.append(f"{d}^{count}" if count > 1 else str(d))
    else:
        d += 1
print(" × ".join(result))

print("\n=== 8.54. Все простые делители ===")
print("8.54 (n=60):")
n = 60
temp = n
d = 2
divisors = []
while temp > 1:
    if temp % d == 0:
        divisors.append(d)
        while temp % d == 0:
            temp //= d
    d += 1
print(f"Простые делители {n}: {divisors}")

print("\n=== 8.55. Числа, взаимно простые с n ===")
print("8.55 (n=12):")
def nod(a, b):
    while b:
        a, b = b, a % b
    return a

n = 12
result = []
for i in range(1, n):
    if nod(i, n) == 1:
        result.append(i)
print(f"Числа, взаимно простые с {n}: {result}")

print("\n=== 8.56. Числа < n, взаимно простые с p ===")
print("8.56 (n=15, p=8):")
n, p = 15, 8
result = []
for i in range(1, n):
    if nod(i, p) == 1:
        result.append(i)
print(f"Числа < {n}, взаимно простые с {p}: {result}")

print("\n=== 8.57. Делители q, взаимно простые с p ===")
print("8.57 (p=6, q=24):")
p, q = 6, 24
result = []
for d in range(1, q + 1):
    if q % d == 0 and nod(d, p) == 1:
        result.append(d)
print(f"Делители {q}, взаимно простые с {p}: {result}")

print("\n=== 8.58. Число, представляемое двумя способами как сумма кубов ===")
print("8.58 (поиск до 1000):")
found = False
for n in range(1, 1000):
    count = 0
    for a in range(1, int(n**(1/3)) + 1):
        for b in range(a, int(n**(1/3)) + 1):
            if a**3 + b**3 == n:
                count += 1
    if count >= 2:
        print(f"Найдено: {n}")
        found = True
        break
if not found:
    print("Не найдено в диапазоне до 1000")

print("\n=== 8.59. Простые несократимые дроби от 0 до 1 ===")
print("8.59 (знаменатель до 8):")
limit = 8
fractions = []
for d in range(2, limit + 1):
    for n in range(1, d):
        if nod(n, d) == 1:
            fractions.append(f"{n}/{d}")
print("Дроби:", ' '.join(sorted(fractions, key=lambda x: eval(x))))

print("\n=== ВСЕ ЗАДАЧИ ГЛАВЫ 8 ВЫПОЛНЕНЫ ===")
