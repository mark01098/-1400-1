# 5.78
import math
n = 1000
dx = math.pi / n
area = 0
for i in range(n):
    x = i * dx
    y = math.sin(x)
    area += y * dx
print(f"5.78: Приближенная площадь арки синусоиды = {area:.6f}")

print()

# 5.79
def f(x):
    return 0.3 * (x - 1)**2 + 4

n = 1000
x_start = 1
x_end = 3
dx = (x_end - x_start) / n
area = 0
for i in range(n):
    x = x_start + i * dx
    if f(x) > 1 and f(x) < 3:
        area += (f(x) - 1) * dx
print(f"5.79: Площадь фигуры = {area:.6f}")

print()

# 5.80
def g(x):
    return 0.5 * (x + 1)**2 + 2

n = 1000
x_start = 0
x_end = 2  
dx = (x_end - x_start) / n
area = 0
for i in range(n):
    x = x_start + i * dx
    if g(x) < 2:
        area += g(x) * dx
print(f"5.80: Площадь фигуры = {area:.6f}")

print()

