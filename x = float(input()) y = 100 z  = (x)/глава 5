# 5.56в
print("Суммы соседних чисел:")
for i in range(len(numbers_n)-1):
    print(f"a{i+1}+a{i+2} = {numbers_n[i]} + {numbers_n[i+1]} = {numbers_n[i] + numbers_n[i+1]}")

# 5.56г
alternating_sum = 0
sign = 1
for x in numbers_n:
    alternating_sum += sign * x
    sign = -sign
print(f"Знакочередующаяся сумма: {alternating_sum}")

print()

