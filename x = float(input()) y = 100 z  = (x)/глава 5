# 5.63
areas = [120, 85, 150, 95, 110, 130, 75, 140, 90, 125]  
yields = [25, 30, 22, 28, 26, 24, 32, 23, 29, 27]       

total_wheat = sum(area * yield_ for area, yield_ in zip(areas, yields))
total_area = sum(areas)
avg_yield = total_wheat / total_area
print(f"Общее количество пшеницы: {total_wheat} ц")
print(f"Средняя урожайность по области: {avg_yield:.2f} ц/га")

print()

# 5.64
populations = [15, 22, 18, 25, 20, 28, 16, 30, 19, 24, 27, 21]  
areas_km2 = [50, 70, 45, 60, 55, 75, 40, 80, 50, 65, 72, 58]  
total_population = sum(populations)
total_area_km2 = sum(areas_km2)
avg_density = total_population / total_area_km2
print(f"Общая численность населения: {total_population} тыс. чел.")
print(f"Общая площадь: {total_area_km2} км²")
print(f"Средняя плотность населения: {avg_density:.2f} тыс. чел./км²")

print()

# 5.65
populations2 = [12, 18, 15, 22, 19, 25, 14, 28, 17, 23, 26, 20]  # тыс. чел.
densities = [0.24, 0.26, 0.33, 0.37, 0.35, 0.33, 0.35, 0.35, 0.34, 0.35, 0.36, 0.34]  # тыс. чел./км²

total_area2 = sum(pop / dens for pop, dens in zip(populations2, densities))
print(f"Общая площадь территории области: {total_area2:.2f} км²")
# 5.66
n = 10
a = [0] * (n + 1)
a[0] = 1
for k in range(1, n + 1):
    a[k] = k * a[k-1] + 1/k
print("5.66: a1-a10:", a[1:])

print()

# 5.67
def fib(n):
    f1, f2 = 1, 1
    for _ in range(n - 2):
        f1, f2 = f2, f1 + f2
    return f2

print("5.67а: 10-й член Фибоначчи:", fib(10))

n_fib = 15
print("5.67б: Первые", n_fib, "членов Фибоначчи:")
f1, f2 = 1, 1
print(f1, f2, end=" ")
for _ in range(n_fib - 2):
    f1, f2 = f2, f1 + f2
    print(f2, end=" ")
print()

n_sum = 10
f1, f2 = 1, 1
sum_fib = 2
for _ in range(n_sum - 2):
    f1, f2 = f2, f1 + f2
    sum_fib += f2
print("5.67в: Сумма первых", n_sum, "членов Фибоначчи:", sum_fib, "(" + ("четная" if sum_fib % 2 == 0 else "нечетная") + ")")

print()

# 5.68
k = 8
num1, num2 = 1, 2
den1, den2 = 1, 1
if k == 1:
    print("5.68а: 1-й член:", f"{num1}/{den1}")
elif k == 2:
    print("5.68а: 2-й член:", f"{num2}/{den2}")
else:
    for i in range(3, k + 1):
        num_new = num1 + num2
        den_new = den1 + den2
        num1, num2 = num2, num_new
        den1, den2 = den2, den_new
    print(f"5.68а: {k}-й член: {num2}/{den2}")

n_seq = 8
print(f"5.68б: Первые {n_seq} членов:")
num1, num2 = 1, 2
den1, den2 = 1, 1
print(f"{num1}/{den1}", f"{num2}/{den2}", end=" ")
for i in range(3, n_seq + 1):
    num_new = num1 + num2
    den_new = den1 + den2
    num1, num2 = num2, num_new
    den1, den2 = den2, den_new
    print(f"{num2}/{den2}", end=" ")
print()

A = 2.5
sum_seq = num1/den1 + num2/den2
num1, num2 = 1, 2
den1, den2 = 1, 1
for i in range(3, n_seq + 1):
    num_new = num1 + num2
    den_new = den1 + den2
    sum_seq += num_new / den_new
    num1, num2 = num2, num_new
    den1, den2 = den2, den_new
print(f"5.68в: Сумма первых {n_seq} членов: {sum_seq:.4f} > {A}?", sum_seq > A)

print()

# 5.69
n_v = 10
v = [0] * (n_v + 1)
v[1] = v[2] = 0
v[3] = 1.5
for i in range(4, n_v + 1):
    v[i] = (i-1)/(i*i+1)*v[i-1] - v[i-2] + v[i-3]
print("5.69: v10 =", v[n_v])

print()

