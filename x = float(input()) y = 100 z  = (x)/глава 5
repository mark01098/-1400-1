# 5.95
number2 = 987654
n_last = 3
last_sum = 0
temp = number2
count = 0
while temp > 0 and count < n_last:
    last_sum += temp % 10
    temp //= 10
    count += 1
print(f"5.95: Сумма последних {n_last} цифр числа {number2} = {last_sum}")

print()

# 5.96
import math
length = 4.5
distance = 3.0
for slide in range(0, 31, 2):  
    slide_distance = slide / 10.0
    if slide_distance >= distance:
        break
    cos_angle = slide_distance / length
    angle = math.degrees(math.acos(cos_angle))
    print(f"После скольжения {slide_distance:.1f} м: угол = {angle:.1f}°")
