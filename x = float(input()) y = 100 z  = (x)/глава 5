print()

# 5.37
x = 2
sum_series2 = 0
sign = 1
for i in range(1, 12):
    term = sign * (i / (i + 1)) * (x ** (i - 1))
    sum_series2 += term
    sign = -sign
print(f"Сумма 1 - 2/3x + 3/4x^2 - ... + 11/12x^10 при x=2: {sum_series2:.6f}")

print()

