# 5.90
n_series = 7
series_sum = 1.0
current_fact = 1.0
for i in range(1, n_series + 1):
    current_fact *= i
    series_sum += 1.0 / current_fact
print(f"5.90: 1 + 1/1! + 1/2! + ... + 1/{n_series}! = {series_sum:.6f}")

print()

# 5.91
x = 2.0
n_power = 5
power_sum = 1.0
current_fact = 1.0
x_power = 1.0
for i in range(1, n_power + 1):
    current_fact *= i
    x_power *= x
    power_sum += x_power / current_fact
print(f"5.91: 1 + x/1! + x²/2! + ... + x^{n_power}/{n_power}! (x={x}) = {power_sum:.6f}")

print()

# 5.92
import math
result = math.sqrt(50)
for i in range(49, 0, -1):
    result = math.sqrt(i + result)
print(f"5.92: √(1+√(2+√(3+...+√50))) = {result:.6f}")

print()

# 5.93а
import math
n_sin = 5
sin_sum = 0
partial_sum = 0
for i in range(1, n_sin + 1):
    partial_sum += math.sin(i)
    sin_sum += 1.0 / partial_sum
print(f"5.93а: 1/sin1 + 1/(sin1+sin2) + ... + 1/(sin1+...+sin{n_sin}) = {sin_sum:.6f}")

# 5.93б
n_sqrt = 5
sqrt_result = math.sqrt(2)
for _ in range(n_sqrt - 1):
    sqrt_result = math.sqrt(2 + sqrt_result)
print(f"5.93б: √(2+√(2+...+√2)) ({n_sqrt} корней) = {sqrt_result:.6f}")

# 5.93в
n_frac = 4
frac_sum = 0
cos_part = 0
sin_part = 0
for i in range(1, n_frac + 1):
    cos_part += math.cos(i)
    sin_part += math.sin(i)
    frac_sum += cos_part / sin_part
print(f"5.93в: cos1/sin1 + (cos1+cos2)/(sin1+sin2) + ... = {frac_sum:.6f}")

# 5.93г
n_nested = 5
nested_result = math.sqrt(3 * n_nested)
for i in range(n_nested - 1, 0, -1):
    nested_result = math.sqrt(3 * i + nested_result)
print(f"5.93г: √(3+√(6+...+√(3n))) (n={n_nested}) = {nested_result:.6f}")

print()

