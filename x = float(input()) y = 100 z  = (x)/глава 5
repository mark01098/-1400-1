# 5.73
area = 100
yield_per_ha = 20
for year in range(2, 9):
    area *= 1.05
    yield_per_ha *= 1.02
    if year >= 4 and year <= 7:
        print(f"5.73б: Площадь в {year} год: {area:.1f} га")
    if year <= 8:
        print(f"5.73а: Урожайность в {year} год: {yield_per_ha:.2f} ц/га")

area = 100
yield_per_ha = 20
total_harvest = 0
for year in range(1, 7):
    total_harvest += area * yield_per_ha
    area *= 1.05
    yield_per_ha *= 1.02
print(f"5.73в: Урожай за 6 лет: {total_harvest:.1f} ц")

print()

# 5.74
from math import pi
r = 5  
thickness = 0.5  
total_volume = 0
for i in range(12):
    volume = 4/3 * pi * r**3
    total_volume += volume / 1000  #
    r += thickness
print(f"5.74: Суммарный объем 12 шаров: {total_volume:.3f} л")

print()

