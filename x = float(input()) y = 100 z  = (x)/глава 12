# 12.126
print("\n126:")
a = [[1,2,3],[4,5,6],[7,8,9]]
total = sum(sum(row) for row in a)
count = len(a) * len(a[0])
avg = total / count
closest_val = a[0][0]
closest_i, closest_j = 0, 0
for i in range(len(a)):
    for j in range(len(a[0])):
        if abs(a[i][j] - avg) < abs(closest_val - avg):
            closest_val = a[i][j]
            closest_i, closest_j = i, j
print("126:", closest_i, closest_j)

# 12.127
print("\n127:")
total = sum(sum(row) for row in a)
print("127:", 1000 <= total <= 9999)

# 12.128
print("\n128:")
train = [[random.randint(0,1) for _ in range(36)] for _ in range(20)]
has_free = False
for row in train:
    if 0 in row:
        has_free = True
        break
print("128:", has_free)

# 12.129
print("\n129:")
income = [[random.randint(100,500) for _ in range(12)] for _ in range(10)]
limit = 40000
total = 0
for row in income:
    total += sum(row)
print("129:", total > limit)

# 12.130 - пропускаем (слишком сложно)

# 12.131
print("\n131:")
magic = [[2,7,6],[9,5,1],[4,3,8]]
n = 3
target_sum = sum(magic[0])
is_magic = True
# Проверка строк
for row in magic:
    if sum(row) != target_sum:
        is_magic = False
        break
# Проверка столбцов
for j in range(n):
    col_sum = 0
    for i in range(n):
        col_sum += magic[i][j]
    if col_sum != target_sum:
        is_magic = False
        break
# Проверка диагоналей
diag1 = sum(magic[i][i] for i in range(n))
diag2 = sum(magic[i][n-1-i] for i in range(n))
if diag1 != target_sum or diag2 != target_sum:
    is_magic = False
print("131:", is_magic)

# 12.132
print("\n132:")
a = [[1,2,3],[4,5,6],[7,8,9]]
seen = set()
has_duplicate = False
for row in a:
    for x in row:
        if x in seen:
            has_duplicate = True
            break
        seen.add(x)
print("132:", has_duplicate)

# 12.133
print("\n133:")
a = [[1,2,3],[4,5,6],[7,8,1]]
count_dict = {}
for row in a:
    for x in row:
        count_dict[x] = count_dict.get(x, 0) + 1
duplicates = [x for x, count in count_dict.items() if count == 2]
print("133:", len(duplicates) == 1)

# 12.134
print("\n134:")
for x, count in count_dict.items():
    if count == 2:
        print("134:", x)
        break

