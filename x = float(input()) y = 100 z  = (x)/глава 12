# 12.266
print("\n266:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
threshold = 4
# а)
col_counts = []
for j in range(len(arr[0])):
    count = 0
    for i in range(len(arr)):
        if arr[i][j] > threshold:
            count += 1
    col_counts.append(count)
print("266a (по столбцам >", threshold, "):", col_counts)

# б)
row_sums = []
for row in arr:
    total = 0
    for x in row:
        if x < threshold:
            total += x
    row_sums.append(total)
print("266b (по строкам <", threshold, "):", row_sums)

# 12.267
print("\n267:")
arr = [[2,-3,4],[-5,6,-7],[8,-9,10]]
# а)
col_sums = []
for j in range(len(arr[0])):
    total = 0
    for i in range(len(arr)):
        if arr[i][j] > 0 and arr[i][j] % 2 == 0:
            total += arr[i][j]
    col_sums.append(total)
print("267a (четные положительные по столбцам):", col_sums)

# б)
row_counts = []
for row in arr:
    count = 0
    for x in row:
        if x < 0 and x % 2 != 0:
            count += 1
    row_counts.append(count)
print("267b (нечетные отрицательные по строкам):", row_counts)

# в)
row_counts2 = []
for row in arr:
    count = 0
    for x in row:
        if x < 0 and (x % 3 == 0 or x % 7 == 0):
            count += 1
    row_counts2.append(count)
print("267c (отрицательные кратные 3 или 7):", row_counts2)

# г)
col_sums2 = []
for j in range(len(arr[0])):
    total = 0
    for i in range(len(arr)):
        if arr[i][j] > 0 and (arr[i][j] % 4 == 0 or arr[i][j] % 5 == 0):
            total += arr[i][j]
    col_sums2.append(total)
print("267d (положительные кратные 4 или 5):", col_sums2)

# 12.268
print("\n268:")
arr = [[-5,10,-3],[8,-2,4],[-6,7,-9]]
# а)
col_max_abs = []
for j in range(len(arr[0])):
    max_abs = abs(arr[0][j])
    for i in range(1, len(arr)):
        if abs(arr[i][j]) > max_abs:
            max_abs = abs(arr[i][j])
    col_max_abs.append(max_abs)
print("268a (макс по модулю по столбцам):", col_max_abs)

# б)
row_max_abs = []
for row in arr:
    max_abs = abs(row[0])
    for x in row[1:]:
        if abs(x) > max_abs:
            max_abs = abs(x)
    row_max_abs.append(max_abs)
print("268b (макс по модулю по строкам):", row_max_abs)

# 12.269
print("\n269:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
# а)
first_even_col = []
for j in range(len(arr[0])):
    found = 0
    for i in range(len(arr)):
        if arr[i][j] % 2 == 0:
            found = arr[i][j]
            break
    first_even_col.append(found)
print("269a (первый четный по столбцам):", first_even_col)

# б)
last_odd_row = []
for row in arr:
    found = 0
    for x in reversed(row):
        if x % 2 != 0:
            found = x
            break
    last_odd_row.append(found)
print("269b (последний нечетный по строкам):", last_odd_row)

