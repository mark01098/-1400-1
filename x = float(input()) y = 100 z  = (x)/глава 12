max_col = len(a[row_num]) - 1 - a[row_num][::-1].index(max_val)
print("a:", max_col)

# б) для столбца col_num
min_val = min(row[col_num] for row in a)
min_row = len(a) - 1
for i in range(len(a)-1, -1, -1):
    if a[i][col_num] == min_val:
        min_row = i
        break
print("b:", min_row)

# 12.89
print("\n89:")
a = [[5,10,5],[3,8,3],[7,12,7]]
# Со списком
