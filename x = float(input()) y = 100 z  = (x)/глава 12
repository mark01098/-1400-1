# 12.166
arr[0][0], arr[2][2] = arr[2][2], arr[0][0]
print("166: поменяны местами элементы главной диагонали")

# 12.167
arr[0][n-1], arr[n-1][0] = arr[n-1][0], arr[0][n-1]
print("167: поменяны местами элементы побочной диагонали")

# Восстановим массив
arr = [[(i*5 + j + 1) for j in range(n)] for i in range(n)]

# 12.168
print(f"168: сумма главной диагонали <= 100? {main_sum <= 100}")

# 12.169
print(f"169: сумма побочной диагонали оканчивается 0? {side_sum % 10 == 0}")

# 12.170
if main_sum > side_sum:
    print("170: сумма главной диагонали больше")
elif main_sum < side_sum:
    print("170: сумма побочной диагонали больше")
else:
    print("170: суммы равны")

import random

# 12.171
print("171:")
n = 5
arr = [[random.randint(1, 30) for _ in range(n)] for _ in range(n)]
print("Массив 5x5:")
for row in arr:
    print(row)

# а)
sum_main_gt20 = 0
for i in range(n):
    if arr[i][i] > 20:
        sum_main_gt20 += arr[i][i]
print("a:", sum_main_gt20)

# б)
sum_side_mod4 = 0
for i in range(n):
    if arr[i][n-1-i] % 4 == 0:
        sum_side_mod4 += arr[i][n-1-i]
print("b:", sum_side_mod4)

# в)
count_side_eq5 = sum(1 for i in range(n) if arr[i][n-1-i] == 5)
print("c:", count_side_eq5)

# г)
count_main_even = sum(1 for i in range(n) if arr[i][i] % 2 == 0)
print("d:", count_main_even)

# 12.172
print("\n172:")
# а)
prod_side_lt10 = 1
for i in range(n):
    if arr[i][n-1-i] < 10:
        prod_side_lt10 *= arr[i][n-1-i]
print("a:", prod_side_lt10)

# б)
sum_main_end7 = sum(arr[i][i] for i in range(n) if arr[i][i] % 10 == 7)
print("b:", sum_main_end7)

# в)
count_main_zero = sum(1 for i in range(n) if arr[i][i] == 0)
print("c:", count_main_zero)

# г)
count_side_odd = sum(1 for i in range(n) if arr[i][n-1-i] % 2 == 1)
print("d:", count_side_odd)

# 12.173
print("\n173:")
# а)
min_main = min(arr[i][i] for i in range(n))
print("a:", min_main)

# б)
max_side = max(arr[i][n-1-i] for i in range(n))
print("b:", max_side)

# 12.174
print("\n174:")
# а)
print("a:", max(arr[i][i] for i in range(n)))

# б)
print("b:", min(arr[i][n-1-i] for i in range(n)))

# 12.175
print("\n175:")
# а)
max_main = arr[0][0]
max_main_index = 0
for i in range(1, n):
    if arr[i][i] > max_main:
        max_main = arr[i][i]
        max_main_index = i
print("a:", max_main_index, max_main_index)

# б)
min_side = arr[0][n-1]
min_side_index = 0
for i in range(1, n):
    if arr[i][n-1-i] < min_side:
        min_side = arr[i][n-1-i]
        min_side_index = i
print("b:", min_side_index, n-1-min_side_index)

# 12.176
