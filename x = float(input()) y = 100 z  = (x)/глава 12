for j in range(len(arr[0])):
    for i in range(len(arr)):
        result_b.append(arr[i][j])
print("262b:", result_b)

# 12.263
print("\n263:")
n = 4
arr = [[i*4 + j + 1 for j in range(n)] for i in range(n)]
print("Массив 4x4:")
for row in arr:
    print(row)

# а)
above_main = []
for i in range(n):
    for j in range(i+1, n):
        above_main.append(arr[i][j])
print("263a (над главной диагональю):", above_main)

# б)
below_main = []
for i in range(1, n):
    for j in range(i):
        below_main.append(arr[i][j])
print("263b (под главной диагональю):", below_main)

# в)
above_side = []
for i in range(n-1):
    for j in range(n-1-i):
        above_side.append(arr[i][j])
print("263c (над побочной диагональю):", above_side)

# г)
below_side = []
for i in range(1, n):
    for j in range(n-i, n):
        below_side.append(arr[i][j])
print("263d (под побочной диагональю):", below_side)

# 12.264
print("\n264:")
arr = [[random.randint(-10, 10) for _ in range(n)] for _ in range(n)]
# а)
multiples_of_3 = []
for row in arr:
    for x in row:
        if x % 3 == 0:
            multiples_of_3.append(x)
print("264a (кратные 3):", multiples_of_3)

# б)
positive = []
others = []
for row in arr:
    for x in row:
        if x > 0:
            positive.append(x)
        else:
            others.append(x)
print("264b положительные:", positive)
print("264b остальные:", others)

