    for j in range(len(arr[0])):
        if arr[i][j] == 0:
            zero_row, zero_col = i, j
            break
    if zero_row != -1:
        break

if zero_row != -1:
    arr[0], arr[zero_row] = arr[zero_row], arr[0]
