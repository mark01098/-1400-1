# 12.96
print("\n96:")
a = [[1,2,3],[4,5,6],[7,8,9]]
# Со списком
row_sums = [sum(row) for row in a]
max_row_val = max(row_sums)
max_row_index = len(row_sums) - 1 - row_sums[::-1].index(max_row_val)
print("a со списком:", max_row_index)

col_sums = []
for j in range(len(a[0])):
    s = 0
    for i in range(len(a)):
        s += a[i][j]
    col_sums.append(s)
min_col_val = min(col_sums)
min_col_index = col_sums.index(min_col_val)
print("b со списком:", min_col_index)

# Без списка
max_row_index = 0
max_row_sum = sum(a[0])
for i in range(1, len(a)):
    s = sum(a[i])
    if s >= max_row_sum:
        max_row_sum = s
        max_row_index = i
print("a без списка:", max_row_index)

min_col_index = 0
min_col_sum = 0
for i in range(len(a)):
    min_col_sum += a[i][0]
for j in range(1, len(a[0])):
    s = 0
    for i in range(len(a)):
        s += a[i][j]
    if s < min_col_sum:
        min_col_sum = s
        min_col_index = j
print("b без списка:", min_col_index)

