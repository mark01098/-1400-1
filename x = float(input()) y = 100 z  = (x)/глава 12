# 12.177
print("\n177:")
n = 5  # нечетное
arr = [[(i*5 + j + 1) for j in range(n)] for i in range(n)]
center = n // 2

# Находим максимальный на диагоналях
max_diag = arr[0][0]
max_i, max_j = 0, 0
for i in range(n):
    if arr[i][i] > max_diag:
        max_diag = arr[i][i]
        max_i, max_j = i, i
    if arr[i][n-1-i] > max_diag:
        max_diag = arr[i][n-1-i]
        max_i, max_j = i, n-1-i

# Меняем с центром
arr[max_i][max_j], arr[center][center] = arr[center][center], arr[max_i][max_j]
print("177: поменяли максимальный на диагоналях с центральным")

# 12.178
arr = [[(i*5 + j + 1) for j in range(n)] for i in range(n)]

# Находим минимальный на диагоналях
min_diag = arr[0][0]
min_i, min_j = 0, 0
for i in range(n):
    if arr[i][i] < min_diag:
        min_diag = arr[i][i]
        min_i, min_j = i, i
    if arr[i][n-1-i] < min_diag:
        min_diag = arr[i][n-1-i]
        min_i, min_j = i, n-1-i

# Меняем с левым нижним углом
arr[min_i][min_j], arr[n-1][0] = arr[n-1][0], arr[min_i][min_j]
print("178: поменяли минимальный на диагоналях с левым нижним углом")

# 12.179
print("\n179:")
n = 5
# а) симметрично относительно главной диагонали
arr_sym_main = [[0]*n for _ in range(n)]
for i in range(n):
    for j in range(n):
        if i <= j:
            arr_sym_main[i][j] = i*5 + j + 1
            arr_sym_main[j][i] = i*5 + j + 1
print("a (симметрия относительно главной диагонали):")
for row in arr_sym_main:
    print(row)

# б) симметрично относительно побочной диагонали
arr_sym_side = [[0]*n for _ in range(n)]
for i in range(n):
    for j in range(n):
        if i + j <= n-1:
            value = i*5 + j + 1
            arr_sym_side[i][j] = value
            arr_sym_side[n-1-j][n-1-i] = value
print("\nb (симметрия относительно побочной диагонали):")
for row in arr_sym_side:
    print(row)

# 12.180
print("\n180:")
n = 18
table = [[0]*n for _ in range(n)]
# Заполняем только над главной диагональю
for i in range(n):
    for j in range(i+1, n):
        table[i][j] = random.choice([0, 1, 3])

print("180a (со списком):")
wins = [0]*n
draws = [0]*n
losses = [0]*n
points = [0]*n

for i in range(n):
    for j in range(i+1, n):
        if table[i][j] == 3:
            wins[i] += 1
            losses[j] += 1
            points[i] += 3
        elif table[i][j] == 1:
            draws[i] += 1
            draws[j] += 1
            points[i] += 1
            points[j] += 1
        else:  # 0
            losses[i] += 1
            wins[j] += 1
            points[j] += 3

for i in range(3):  # покажем для первых 3 команд
    print(f"Команда {i}: выигрыши={wins[i]}, ничьи={draws[i]}, проигрыши={losses[i]}, очки={points[i]}")

print("\n180b (без списка):")
for i in range(3):  # для первых 3 команд
    win_count = 0
    draw_count = 0
    loss_count = 0
    point_count = 0
    
    # Просматриваем строку i
    for j in range(i+1, n):
        if table[i][j] == 3:
            win_count += 1
            point_count += 3
        elif table[i][j] == 1:
            draw_count += 1
            point_count += 1
        else:
            loss_count += 1
    
    # Просматриваем столбец i (игры, где команда i была гостем)
    for k in range(i):
        if table[k][i] == 0:
            win_count += 1
            point_count += 3
        elif table[k][i] == 1:
            draw_count += 1
            point_count += 1
        else:
            loss_count += 1
    
    print(f"Команда {i}: выигрыши={win_count}, ничьи={draw_count}, проигрыши={loss_count}, очки={point_count}")

