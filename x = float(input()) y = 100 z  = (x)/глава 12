# 12.70
print("\n70:")
games = [[random.randint(0,5) for _ in range(22)],
         [random.randint(0,5) for _ in range(22)]]

wins = 0
draws = 0
loses = 0
diff_ge_3 = 0
points = 0

for j in range(22):
    if games[0][j] > games[1][j]:
        wins += 1
        points += 3
        print("выигрыш", end=" ")
    elif games[0][j] == games[1][j]:
        draws += 1
        points += 1
        print("ничья", end=" ")
    else:
        loses += 1
        print("проигрыш", end=" ")
    
    if games[0][j] - games[1][j] >= 3:
        diff_ge_3 += 1

print("\na: вывод выше")
print("b:", wins)
print("c:", wins, loses)
print("d:", wins, draws, loses)
print("e:", diff_ge_3)
print("f:", points)

# 12.71
print("\n71:")
a = [[10,35,20],[25,40,15],[30,45,50]]
# а)
print("a:", sum(x for row in a for x in row if x > 30))

# б)
print("b:", sum(1 for row in a for x in row if x%2==1))

# в)
even = [x for row in a for x in row if x%2==0]
if even:
    print("c:", sum(even)/len(even))

# г)
s = 2
sum_s = 0
for i in range(len(a)):
    for j in range(len(a[0])):
        if i + j == s:
            sum_s += a[i][j]
print("d:", sum_s)

# 12.72
print("\n72:")
a = [[45,20,60],[30,55,10],[25,65,40]]
# а)
print("a:", sum(x for row in a for x in row if x%2==0))

# б)
print("b:", sum(1 for row in a for x in row if x < 50))

# в)
