min_row = 0
max_tickets = sum(seats[0])
min_tickets = sum(seats[0])
for i in range(1, len(seats)):
    s = sum(seats[i])
    if s > max_tickets:
        max_tickets = s
        max_row = i
    if s < min_tickets:
        min_tickets = s
        min_row = i
print("a без списка:", max_row + 1)
print("b без списка:", min_row + 1)

# 12.100
print("\n100:")
n = 17
table = [[0]*n for _ in range(n)]
for i in range(n):
    for j in range(n):
        if i != j:
            table[i][j] = random.choice([0,1,3])
# Со списком
team_scores = [sum(row) for row in table]
print("a со списком:", max(team_scores))
min_score = min(team_scores)
last_team = team_scores.index(min_score)
print("b со списком:", last_team + 1)

# Без списка
max_score = sum(table[0])
min_score = sum(table[0])
last_team = 0
champion_team = 0
for i in range(1, n):
    s = sum(table[i])
    if s > max_score:
        max_score = s
        champion_team = i
    if s < min_score:
        min_score = s
        last_team = i
print("a без списка:", max_score)
print("b без списка:", last_team + 1)

