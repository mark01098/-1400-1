for row in b:
    print(row)

# 12.26
print("26:")
n = 8
c = [[0]*n for _ in range(n)]
for i in range(n):
    for j in range(n):
        if (i+j)%2==0:
            c[i][j]=1
for row in c:
    print(row)

# 12.27
print("27:")
m,n = 3,4
d = list(range(1, m*n+1))
e = []
for i in range(m):
    e.append(d[i*n:(i+1)*n])
for row in e:
    print(row)

# 12.28
print("28:")
n = 5
f = [[0]*n for _ in range(n)]
num = 1
top = 0
bottom = n-1
left = 0
right = n-1

while num <= n*n:
    for j in range(left, right+1):
        f[top][j] = num
        num += 1
    top += 1
    
    for i in range(top, bottom+1):
        f[i][right] = num
        num += 1
    right -= 1
    
    for j in range(right, left-1, -1):
        f[bottom][j] = num
        num += 1
    bottom -= 1
    
    for i in range(bottom, top-1, -1):
        f[i][left] = num
        num += 1
    left += 1

for row in f:
    print(row)

# 12.29
print("29:")
n = 7
g = [[0]*n for _ in range(n)]
num = 1
top = 0
bottom = n-1
left = 0
right = n-1

while num <= n*n:
    for j in range(left, right+1):
        g[top][j] = num
        num += 1
    top += 1
    
    for i in range(top, bottom+1):
        g[i][right] = num
        num += 1
    right -= 1
    
    for j in range(right, left-1, -1):
        g[bottom][j] = num
        num += 1
    bottom -= 1
    
    for i in range(bottom, top-1, -1):
        g[i][left] = num
        num += 1
    left += 1

for row in g:
    print(row)

# 12.30
print("30:")
n = 9
h = [[0]*n for _ in range(n)]
num = 1
top = 0
bottom = n-1
left = 0
right = n-1

while num <= n*n:
    for j in range(left, right+1):
        h[top][j] = num
        num += 1
    top += 1
    
    for i in range(top, bottom+1):
        h[i][right] = num
        num += 1
    right -= 1
    
    for j in range(right, left-1, -1):
        h[bottom][j] = num
        num += 1
    bottom -= 1
    
    for i in range(bottom, top-1, -1):
        h[i][left] = num
        num += 1
    left += 1

res = []
top = 0
bottom = n-1
left = 0
right = n-1

while len(res) < n*n:
    for j in range(left, right+1):
        res.append(h[top][j])
    top += 1
    
    for i in range(top, bottom+1):
        res.append(h[i][right])
    right -= 1
    
    for j in range(right, left-1, -1):
        res.append(h[bottom][j])
    bottom -= 1
    
    for i in range(bottom, top-1, -1):
        res.append(h[i][left])
    left += 1

print(res[:20], "...")

