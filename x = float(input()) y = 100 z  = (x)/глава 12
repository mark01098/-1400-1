# 12.145
print("\n145:")
a = [[1,2,-3],[4,-5,6],[-7,8,-9]]
last_i, last_j = -1, -1
for i in range(len(a)):
    for j in range(len(a[0])):
        if a[i][j] < 0:
            # ищем самый нижний и самый правый
            if i > last_i or (i == last_i and j > last_j):
                last_i, last_j = i, j
print("145:", last_i, last_j)

# 12.146
print("\n146:")
a = [[1,-2,3],[-4,5,6],[7,8,-9]]
# а)
for i in range(len(a)):
    for j in range(len(a[i])):
        if a[i][j] < 0:
            print(f"строка {i}: первый отрицательный на позиции {j}")
            break

# б)
for i in range(len(a)):
    for j in range(len(a[i])-1, -1, -1):
        if a[i][j] % 2 == 0:
            print(f"строка {i}: последний четный на позиции {j}")
            break

# 12.147
print("\n147:")
a = [[1,2,3],[4,5,6],[7,8,9]]
# а)
for j in range(len(a[0])):
    for i in range(len(a)):
        if a[i][j] % 2 == 1:
            print(f"столбец {j}: первый нечетный на позиции {i}")
            break

# б)
for j in range(len(a[0])):
    for i in range(len(a)-1, -1, -1):
        if a[i][j] > 0:
            print(f"столбец {j}: последний положительный на позиции {i}")
            break

# 12.148
print("\n148:")
a = [[-1,-2,-3],[1,2,3],[4,5,6]]
digit = 3
for i in range(len(a)):
    # а)
    has_positive = False
    for x in a[i]:
        if x > 0:
            has_positive = True
            break
    print(f"строка {i}: есть положительные? {has_positive}")
    
    # б)
    has_digit = False
    for x in a[i]:
        if x % 10 == digit:
            has_digit = True
            break
    print(f"строка {i}: есть элементы с последней цифрой {digit}? {has_digit}")
    
    # в)
    is_sorted = True
    for j in range(len(a[i])-1):
        if a[i][j] > a[i][j+1]:
            is_sorted = False
            break
    print(f"строка {i}: отсортирована по возрастанию? {is_sorted}")
    
    # г)
    has_duplicate = len(set(a[i])) != len(a[i])
    print(f"строка {i}: есть одинаковые элементы? {has_duplicate}")

# 12.149
print("\n149:")
d = 5
for j in range(len(a[0])):
    # а)
    has_greater = False
    for i in range(len(a)):
        if a[i][j] > d:
            has_greater = True
            break
    print(f"столбец {j}: есть элементы > {d}? {has_greater}")
    
    # б)
    has_odd = False
    for i in range(len(a)):
        if a[i][j] % 2 == 1:
            has_odd = True
            break
    print(f"столбец {j}: есть нечетные? {has_odd}")
    
    # в)
    is_sorted = True
    for i in range(len(a)-1):
        if a[i][j] < a[i+1][j]:
            is_sorted = False
            break
    print(f"столбец {j}: отсортирован по убыванию? {is_sorted}")
    
    # г)
    col_values = [a[i][j] for i in range(len(a))]
    has_duplicate = len(set(col_values)) != len(col_values)
    print(f"столбец {j}: есть одинаковые? {has_duplicate}")

# 12.150
print("\n150:")
a = [[1,2,3],[-4,5,6],[7,8,9],[2,4,6],[3,6,9]]
# а)
for i in range(len(a)):
    all_positive = True
    for x in a[i]:
        if x <= 0:
            all_positive = False
            break
    if all_positive:
        print(f"a: строка {i}")
        break

# б)
for i in range(len(a)):
    all_non_negative = True
    for x in a[i]:
        if x < 0:
            all_non_negative = False
            break
    if all_non_negative:
        print(f"b: строка {i}")
        break

# в)
for i in range(len(a)):
    all_even = True
    for x in a[i]:
        if x % 2 != 0:
            all_even = False
            break
    if all_even:
        print(f"c: строка {i}")
        break

# г)
num = 3
for i in range(len(a)):
    all_multiple = True
    for x in a[i]:
        if x % num != 0:
            all_multiple = False
            break
    if all_multiple:
        print(f"d: строка {i}")
        break

# 12.151
print("\n151:")
# а)
for j in range(len(a[0])):
    all_odd = True
    for i in range(len(a)):
        if a[i][j] % 2 == 0:
            all_odd = False
            break
    if all_odd:
        print(f"a: столбец {j}")
        break

# б)
for j in range(len(a[0])):
    all_greater = True
    for i in range(len(a)):
        if a[i][j] <= 10:
            all_greater = False
            break
    if all_greater:
        print(f"b: столбец {j}")
        break

# в)
a_val, b_val = 2, 8
for j in range(len(a[0])):
    all_in_range = True
    for i in range(len(a)):
        if not (a_val <= a[i][j] <= b_val):
            all_in_range = False
            break
    if all_in_range:
        print(f"c: столбец {j}")
        break

# г)
num = 3
for j in range(len(a[0])):
    all_multiple = True
    for i in range(len(a)):
        if a[i][j] % num != 0:
            all_multiple = False
            break
    if all_multiple:
        print(f"d: столбец {j}")
        break

# 12.152
print("\n152:")
a = [[1,3,5],[2,4,6],[-1,-2,-3],[1,-2,3],[5,5,1]]
# а)
for row in a:
    all_odd = True
    for x in row:
        if x % 2 == 0:
            all_odd = False
            break
    if all_odd:
        print("a: есть строка только из нечетных")
        break

# б)
num1, num2 = 2, 3
for row in a:
    all_multiple = True
    for x in row:
        if x % num1 != 0 and x % num2 != 0:
            all_multiple = False
            break
    if all_multiple:
        print("b: есть строка только из кратных", num1, "или", num2)
        break

# в)
for row in a:
    all_negative = True
    for x in row:
        if x >= 0:
            all_negative = False
            break
    if all_negative:
        print("c: есть строка только из отрицательных")
        break

# г)
for row in a:
    pos_count = sum(1 for x in row if x > 0)
    neg_count = sum(1 for x in row if x < 0)
    if pos_count > neg_count:
        print("d: есть строка с больше положительных чем отрицательных")
        break

# д)
for row in a:
    if len(set(row)) != len(row):
        print("e: есть строка с одинаковыми элементами")
        break

# е)
max_val = max(max(row) for row in a)
for row in a:
    count_max = sum(1 for x in row if x == max_val)
    if count_max >= 2:
        print("f: есть строка с хотя бы двумя максимальными элементами")
        break

# 12.153
print("\n153:")
# а)
for j in range(len(a[0])):
    all_zero = True
    for i in range(len(a)):
        if a[i][j] != 0:
            all_zero = False
            break
    if all_zero:
        print("a: есть столбец только из нулей")
        break

# б)
b_val = 5
for j in range(len(a[0])):
    all_in_range = True
    for i in range(len(a)):
        if not (0 <= a[i][j] <= b_val):
            all_in_range = False
            break
    if all_in_range:
        print("b: есть столбец только из элементов от 0 до", b_val)
        break

# в)
for j in range(len(a[0])):
    all_even = True
    for i in range(len(a)):
        if a[i][j] % 2 != 0:
            all_even = False
            break
    if all_even:
        print("c: есть столбец только из четных")
        break

# г)
for j in range(len(a[0])):
    pos_count = sum(1 for i in range(len(a)) if a[i][j] > 0)
    neg_count = sum(1 for i in range(len(a)) if a[i][j] < 0)
    if pos_count == neg_count:
        print("d: есть столбец с равным количеством положительных и отрицательных")
        break

# д)
for j in range(len(a[0])):
    col_values = [a[i][j] for i in range(len(a))]
    if len(set(col_values)) != len(col_values):
        print("e: есть столбец с одинаковыми элементами")
        break

# е)
min_val = min(min(row) for row in a)
for j in range(len(a[0])):
    count_min = sum(1 for i in range(len(a)) if a[i][j] == min_val)
    if count_min >= 3:
        print("f: есть столбец с хотя бы тремя минимальными элементами")
        break

# 12.154-12.170 - квадратные массивы
print("\n154-170 (квадратные массивы):")
n = 5
arr = [[(i*5 + j + 1) for j in range(n)] for i in range(n)]
print("Массив 5x5:")
for row in arr:
    print(row)

# 12.154
row_num = 2
print(f"154: элемент главной диагонали в строке {row_num}: {arr[row_num][row_num]}")

# 12.155
col_num = 2
print(f"155: элемент побочной диагонали в столбце {col_num}: {arr[col_num][n-1-col_num]}")

# 12.156
print("156a:", end=" ")
for i in range(n):
    print(arr[i][i], end=" ")
print()

print("156b:", end=" ")
for i in range(n-1, -1, -1):
    print(arr[i][n-1-i], end=" ")
print()

# 12.157
print("157a:", end=" ")
for i in range(n):
    print(arr[i][n-1-i], end=" ")
print()

print("157b:", end=" ")
for i in range(n-1, -1, -1):
    print(arr[i][i], end=" ")
print()

# 12.158
print(f"158: сумма элементов главной диагонали: {arr[0][0] + arr[2][2]}")

# 12.159
print(f"159: произведение элементов побочной диагонали: {arr[0][n-1] * arr[n-1][0]}")

# 12.160
for i in range(n):
    arr[i][i] = 0
print("160: главная диагональ заменена нулями")

# 12.161
for i in range(n):
    arr[i][n-1-i] = 100
print("161: побочная диагональ заменена на 100")

# Восстановим массив
arr = [[(i*5 + j + 1) for j in range(n)] for i in range(n)]

# 12.162
main_sum = sum(arr[i][i] for i in range(n))
side_sum = sum(arr[i][n-1-i] for i in range(n))
print(f"162a: сумма главной диагонали: {main_sum}")
print(f"162b: сумма побочной диагонали: {side_sum}")

# 12.163
print(f"163a: среднее главной диагонали: {main_sum/n}")
print(f"163b: среднее побочной диагонали: {side_sum/n}")

# 12.164
print("164:")
# Со списком
sums = []
for i in range(n):
    sums.append(arr[i][i] + arr[i][n-1-i])
print("со списком:", sums)

# Без списка
print("без списка:", end=" ")
for i in range(n):
    print(arr[i][i] + arr[i][n-1-i], end=" ")
print()

