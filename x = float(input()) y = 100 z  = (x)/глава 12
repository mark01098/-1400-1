all_vals = []
for row in a:
    all_vals.extend(row)
print("a:", max(all_vals))
print("b:", min(all_vals))

# Без списка
print("Без списка:")
max_val = a[0][0]
min_val = a[0][0]
for row in a:
    max_val = max(max_val, max(row))
    min_val = min(min_val, min(row))
print("a:", max_val)
print("b:", min_val)

# 12.90
print("\n90:")
a = [[5,10,15,5],[3,8,3,8],[7,12,7,12]]
print("Со списком:")
for i in range(len(a)):
    print(f"Строка {i}: max={max(a[i])}, min={min(a[i])}")

print("Без списка:")
for i in range(len(a)):
    row_max = a[i][0]
    row_min = a[i][0]
    for x in a[i]:
        if x > row_max:
            row_max = x
        if x < row_min:
            row_min = x
    print(f"Строка {i}: max={row_max}, min={row_min}")

