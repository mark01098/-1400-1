# 12.258
print("\n258:")
arr1 = [[1,2,3],[4,5,6]]
arr2 = [[7,8,9],[10,11,12]]

# а)
arr3 = [[0]*len(arr1[0]) for _ in range(len(arr1))]
for i in range(len(arr1)):
    for j in range(len(arr1[0])):
        arr3[i][j] = arr1[i][j] + arr2[i][j]
print("258a:", arr3)

# б)
arr3 = [[0]*len(arr1[0]) for _ in range(len(arr1))]
for i in range(len(arr1)):
    for j in range(len(arr1[0])):
        if (arr1[i][j] > 0 and arr2[i][j] > 0) or (arr1[i][j] < 0 and arr2[i][j] < 0):
            arr3[i][j] = 100
print("258b:", arr3)

# 12.259
print("\n259:")
# а)
arr3 = [[0]*len(arr1[0]) for _ in range(len(arr1))]
for i in range(len(arr1)):
    for j in range(len(arr1[0])):
        arr3[i][j] = arr1[i][j] - arr2[i][j]
print("259a:", arr3)

# б)
arr3 = [[0]*len(arr1[0]) for _ in range(len(arr1))]
for i in range(len(arr1)):
    for j in range(len(arr1[0])):
        if arr1[i][j] > 50 and arr2[i][j] > 50:
            arr3[i][j] = 13
        else:
            arr3[i][j] = 12
print("259b:", arr3)

# 12.260
print("\n260:")
arr1 = [[random.randint(0, 10) for _ in range(28)] for _ in range(12)]
arr2 = [[random.randint(0, 10) for _ in range(28)] for _ in range(12)]
arr3 = [[0]*28 for _ in range(12)]
for i in range(12):
    for j in range(28):
        arr3[i][j] = arr2[i][j] - arr1[i][j]  # изменение осадков
print("260: создан массив изменений осадков")

import random

# 12.261
print("261:")
arr1 = [[random.randint(-10, 30) for _ in range(28)] for _ in range(12)]
arr2 = [[random.randint(-10, 30) for _ in range(28)] for _ in range(12)]
arr3 = [[0]*28 for _ in range(12)]
for i in range(12):
    for j in range(28):
        if arr1[i][j] != 0:
            arr3[i][j] = ((arr2[i][j] - arr1[i][j]) / abs(arr1[i][j])) * 100
        else:
            arr3[i][j] = 0
print("261: создан массив изменений температуры в %")

# 12.262
print("\n262:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
# а)
result_a = []
for row in arr:
    result_a.extend(row)
print("262a:", result_a)

# б)
