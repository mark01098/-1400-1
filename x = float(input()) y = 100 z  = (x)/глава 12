# 12.200
print("\n200:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
k, s = 1, 2
# Меняем знак в строке k
for j in range(len(arr[0])):
    if j != s:  # не меняем элемент на пересечении
        arr[k][j] = -arr[k][j]

# Меняем знак в столбце s
for i in range(len(arr)):
    if i != k:  # не меняем элемент на пересечении
        arr[i][s] = -arr[i][s]

print(f"Строка {k} и столбец {s} (кроме пересечения) заменены на противоположные:")
for row in arr:
    print(row)

