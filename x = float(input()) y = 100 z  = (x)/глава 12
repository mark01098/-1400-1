# 12.201
print("201:")
arr = [[3,5,2],[8,1,4],[6,9,7]]
for i in range(len(arr)):
    max_val = max(arr[i])
    max_index = arr[i].index(max_val)
    arr[i][0], arr[i][max_index] = arr[i][max_index], arr[i][0]
print("201:", arr)

# 12.202
print("\n202:")
arr = [[3,5,2],[8,1,4],[6,9,7]]
for j in range(len(arr[0])):
    # Находим минимальный в столбце
    min_val = arr[0][j]
    min_index = 0
    for i in range(1, len(arr)):
        if arr[i][j] < min_val:
            min_val = arr[i][j]
            min_index = i
    # Меняем с последним
    arr[min_index][j], arr[-1][j] = arr[-1][j], arr[min_index][j]
print("202:", arr)

