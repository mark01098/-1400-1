import random

# 12.62
print("62:")
a = [[2,4,6],[1,3,5],[7,8,9]]
# а) сумма строк
print("a со списком:")
sum_rows = []
for row in a:
    sum_rows.append(sum(row))
print(sum_rows)

print("a без списка:")
for row in a:
    print(sum(row), end=" ")
print()

# б) сумма столбцов
print("b со списком:")
sum_cols = []
for j in range(len(a[0])):
    s = 0
    for i in range(len(a)):
        s += a[i][j]
    sum_cols.append(s)
print(sum_cols)

print("b без списка:")
for j in range(len(a[0])):
    s = 0
    for i in range(len(a)):
        s += a[i][j]
    print(s, end=" ")
print()

