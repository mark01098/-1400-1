# 12.243
print("\n243:")
arr = [[1,0,3],[4,0,0],[7,8,9]]
zero_limit = 2
sum_limit = 10

# а)
target_row = -1
for i in range(len(arr)):
    if sum(1 for x in arr[i] if x == 0) == zero_limit:
        target_row = i
        break

if target_row != -1:
    arr.append([0]*len(arr[0]))
    for i in range(len(arr)-1, target_row+1, -1):
        arr[i] = arr[i-1][:]
    arr[target_row+1] = [0]*len(arr[0])
print("243a:", arr)

# б)
arr = [[1,2,3],[4,5,6],[7,8,9]]
target_col = -1
for j in range(len(arr[0])):
    col_sum = sum(arr[i][j] for i in range(len(arr)))
    if col_sum <= sum_limit:
        target_col = j
        break

if target_col != -1:
    for i in range(len(arr)):
        arr[i].append(0)
        for j in range(len(arr[i])-1, target_col+1, -1):
            arr[i][j] = arr[i][j-1]
        arr[i][target_col+1] = 10
print("243b:", arr)

# 12.244
print("\n244:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
s1, s2 = 1, 2
k1 = 1

# а)
# Вставляем перед s2
arr.append([0]*len(arr[0]))
for i in range(len(arr)-1, s2, -1):
    arr[i] = arr[i-1][:]
arr[s2] = [0]*len(arr[0])

# Вставляем перед s1 (учитываем сдвиг)
arr.append([0]*len(arr[0]))
for i in range(len(arr)-1, s1+1, -1):  # s1 увеличился на 1
    arr[i] = arr[i-1][:]
arr[s1+1] = [0]*len(arr[0])
print("244a:", arr)

# б)
arr = [[1,2,3],[4,5,6],[7,8,9]]
# Вставляем после k1
for i in range(len(arr)):
    arr[i].append(0)
    for j in range(len(arr[i])-1, k1+1, -1):
        arr[i][j] = arr[i][j-1]
    arr[i][k1+1] = 1

# Вставляем перед k1 (учитываем сдвиг)
for i in range(len(arr)):
    arr[i].append(0)
    for j in range(len(arr[i])-1, k1+1, -1):  # k1 увеличился на 1
        arr[i][j] = arr[i][j-1]
    arr[i][k1+1] = 1
print("244b:", arr)

# 12.245
print("\n245:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
s = 1
k = 1

# а) - имитация ввода
