# 12.65
print("\n65:")
group1 = [[random.randint(3,5) for _ in range(3)] for _ in range(20)]
group2 = [[random.randint(3,5) for _ in range(3)] for _ in range(20)]
group3 = [[random.randint(3,5) for _ in range(3)] for _ in range(20)]

avg1 = sum(sum(row) for row in group1)/60
avg2 = sum(sum(row) for row in group2)/60
avg3 = sum(sum(row) for row in group3)/60

if avg1 > avg2 and avg1 > avg3:
    print("Первая группа лучшая")
elif avg2 > avg1 and avg2 > avg3:
    print("Вторая группа лучшая")
else:
    print("Третья группа лучшая")

