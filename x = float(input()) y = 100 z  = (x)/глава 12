# 12.275
print("\n275:")
char_arr = [['н','е','б','о',' ','е'],
            ['с','о','л','н','ц','е'],
            ['в','е','т','е','р','н']]

for i in range(len(char_arr)):
    if 'н' in char_arr[i]:
        n_index = char_arr[i].index('н')
        e_count = 0
        for j in range(n_index+1, len(char_arr[i])):
            if char_arr[i][j] == 'е':
                e_count += 1
        print(f"275 строка {i}: е справа от н: {e_count}")

# 12.276
print("\n276:")
char_arr = [['с','ш','а','р','с'],
            ['щ','и','ш','с','а'],
            ['с','о','к','ш',' ']]

for i in range(len(char_arr)):
    if 'ш' in char_arr[i]:
        sh_index = char_arr[i].index('ш')
        s_count = 0
        for j in range(sh_index):
            if char_arr[i][j] == 'с':
                s_count += 1
        print(f"276 строка {i}: с слева от ш: {s_count}")

# 12.277
print("\n277:")
char_arr = [['д','о','м','а',' '],
            ['с','а','д',' ',' '],
            ['д','е','н','ь','а']]

has_a_after_d = False
for i in range(len(char_arr)):
    if 'д' in char_arr[i] and 'а' in char_arr[i]:
        d_index = char_arr[i].index('д')
        a_indices = [j for j, ch in enumerate(char_arr[i]) if ch == 'а']
        for a_index in a_indices:
            if a_index > d_index:
                has_a_after_d = True
                print(f"277: в строке {i} а справа от д")
                break
if not has_a_after_d:
    print("277: нет строк с а справа от д")

# 12.278
print("\n278:")
char_arr = [['м','о','л','о','к','о'],
            ['о','к','н','о',' ',' '],
            ['о','р','е','о','л','о']]

