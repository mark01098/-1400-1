# 12.98
print("\n98:")
house = [[random.randint(1,5) for _ in range(4)] for _ in range(12)]
# Со списком
floor_sums = [sum(row) for row in house]
min_floor_index = floor_sums.index(min(floor_sums))
max_floor_index = floor_sums.index(max(floor_sums))
print("a со списком:", min_floor_index + 1)
print("b со списком:", max_floor_index + 1)

# Без списка
min_floor = 0
max_floor = 0
min_sum = sum(house[0])
max_sum = sum(house[0])
for i in range(1, len(house)):
    s = sum(house[i])
    if s < min_sum:
        min_sum = s
        min_floor = i
    if s > max_sum:
        max_sum = s
        max_floor = i
print("a без списка:", min_floor + 1)
print("b без списка:", max_floor + 1)

