# 12.283
print("\n283:")
char_arr = [['а',' ','ш','щ','1'],
            ['б',' ',' ','щ','2'],
            ['в','ш','щ',' ','3']]

# а)
max_spaces_col = -1
max_spaces_count = 0
for j in range(len(char_arr[0])):
    spaces = sum(1 for i in range(len(char_arr)) if char_arr[i][j] == ' ')
    if spaces > max_spaces_count:
        max_spaces_count = spaces
        max_spaces_col = j
print(f"283a столбец с макс пробелами: {max_spaces_col+1}")

# б)
max_sh_shch = -1
max_sh_shch_count = 0
for i in range(len(char_arr)):
    count = char_arr[i].count('ш') + char_arr[i].count('щ')
    if count >= max_sh_shch_count:
        max_sh_shch_count = count
        max_sh_shch = i
print(f"283b последняя строка с макс ш/щ: {max_sh_shch+1}")

# в)
max_digits = -1
max_digits_count = 0
for i in range(len(char_arr)):
    digits = sum(1 for ch in char_arr[i] if ch.isdigit())
    if digits > max_digits_count:
        max_digits_count = digits
        max_digits = i
print(f"283c первая строка с макс цифр: {max_digits+1}")

# 12.284
print("\n284:")
n = 8
chess = [[' ']*n for _ in range(n)]
for i in range(n):
    for j in range(n):
        if (i + j) % 2 == 0:
            chess[i][j] = 'x'
print("284 шахматная доска:")
for row in chess:
    print(''.join(row))

# 12.285
print("\n285:")
n = 8
board = [['0']*n for _ in range(n)]
v, g = 3, 4  # вертикаль 3, горизонталь 4 (индексы: v-1, g-1)
vi, gi = v-1, g-1

# а) ладья
board_rook = [row[:] for row in board]
board_rook[gi][vi] = 'л'
for i in range(n):
    if i != gi:
        board_rook[i][vi] = '*'
    if i != vi:
        board_rook[gi][i] = '*'
print("285a ладья:")
for row in board_rook:
    print(''.join(row))

# б) слон
board_bishop = [row[:] for row in board]
board_bishop[gi][vi] = 'с'
for i in range(1, n):
    if gi-i >= 0 and vi-i >= 0:
        board_bishop[gi-i][vi-i] = '*'
    if gi-i >= 0 and vi+i < n:
        board_bishop[gi-i][vi+i] = '*'
    if gi+i < n and vi-i >= 0:
        board_bishop[gi+i][vi-i] = '*'
    if gi+i < n and vi+i < n:
        board_bishop[gi+i][vi+i] = '*'
print("\n285b слон:")
for row in board_bishop:
    print(''.join(row))

# в) ферзь
board_queen = [row[:] for row in board]
board_queen[gi][vi] = 'ф'
# ладья + слон
for i in range(n):
    if i != gi:
        board_queen[i][vi] = '*'
    if i != vi:
        board_queen[gi][i] = '*'
for i in range(1, n):
    if gi-i >= 0 and vi-i >= 0:
        board_queen[gi-i][vi-i] = '*'
    if gi-i >= 0 and vi+i < n:
        board_queen[gi-i][vi+i] = '*'
    if gi+i < n and vi-i >= 0:
        board_queen[gi+i][vi-i] = '*'
    if gi+i < n and vi+i < n:
        board_queen[gi+i][vi+i] = '*'
print("\n285c ферзь:")
for row in board_queen:
    print(''.join(row))

# г) конь
board_knight = [row[:] for row in board]
board_knight[gi][vi] = 'к'
moves = [(-2,-1),(-2,1),(-1,-2),(-1,2),(1,-2),(1,2),(2,-1),(2,1)]
for move in moves:
    ni, nj = gi + move[0], vi + move[1]
    if 0 <= ni < n and 0 <= nj < n:
        board_knight[ni][nj] = '*'
print("\n285d конь:")
for row in board_knight:
    print(''.join(row))

# 12.286 - пропускаем (очень сложно)

# 12.287
print("\n287:")
n = 18
results = [['']*n for _ in range(n)]
# Заполняем над главной диагональю
for i in range(n):
    for j in range(i+1, n):
        home = random.randint(0, 3)
        away = random.randint(0, 3)
        results[i][j] = f"{home}:{away}"

print("287 со списком:")
wins = [0]*n
draws = [0]*n
losses = [0]*n
points = [0]*n
goals_for = [0]*n
goals_against = [0]*n

for i in range(n):
    for j in range(i+1, n):
        if results[i][j]:
            home_goals, away_goals = map(int, results[i][j].split(':'))
            goals_for[i] += home_goals
            goals_against[i] += away_goals
            goals_for[j] += away_goals
            goals_against[j] += home_goals
            
            if home_goals > away_goals:
                wins[i] += 1
                losses[j] += 1
                points[i] += 3
            elif home_goals == away_goals:
                draws[i] += 1
                draws[j] += 1
                points[i] += 1
                points[j] += 1
            else:
                losses[i] += 1
                wins[j] += 1
                points[j] += 3

for i in range(3):  # покажем для первых 3 команд
    print(f"Команда {i+1}: В={wins[i]}, Н={draws[i]}, П={losses[i]}, О={points[i]}, ГЗ={goals_for[i]}, ГП={goals_against[i]}")

print("\n287 без списков (для команды 1):")
win1 = draw1 = loss1 = point1 = gf1 = ga1 = 0
# Просматриваем строку 0
for j in range(1, n):
    if results[0][j]:
        h, a = map(int, results[0][j].split(':'))
        gf1 += h
        ga1 += a
        if h > a:
            win1 += 1
            point1 += 3
        elif h == a:
            draw1 += 1
            point1 += 1
        else:
            loss1 += 1

for i in range(1, n):
    if results[i][0]:
        a, h = map(int, results[i][0].split(':'))  # поменяны местами
        gf1 += h
        ga1 += a
        if h > a:
            win1 += 1
            point1 += 3
        elif h == a:
            draw1 += 1
            point1 += 1
        else:
            loss1 += 1

print(f"Команда 1: В={win1}, Н={draw1}, П={loss1}, О={point1}, ГЗ={gf1}, ГП={ga1}")
