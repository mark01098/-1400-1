    row = []
    for j in range(1,10):
        row.append(i*j)
    t.append(row)
for row in t:
    print(row)

# 12.23
print("23a:")
n = 7
for i in range(n):
    for j in range(n):
        if i==j or i+j==n-1:
            print(1, end=" ")
        else:
            print(0, end=" ")
    print()

# 12.24
print("24a:")
n = 6
p = [[1]*n for _ in range(n)]
for i in range(1,n):
    for j in range(1,n):
        p[i][j] = p[i-1][j] + p[i][j-1]
for row in p:
    print(row)

