# 12.231
print("231:")
arr = [[50, 120, 30], [80, 90, 110], [70, 60, 95]]
# Находим последний элемент > 100
last_col = -1
for i in range(len(arr)):
    for j in range(len(arr[0])):
        if arr[i][j] > 100:
            last_col = j

if last_col != -1 and last_col != 1:  # если не тот же самый столбец
    for i in range(len(arr)):
        arr[i][1], arr[i][last_col] = arr[i][last_col], arr[i][1]
print("231:", arr)

