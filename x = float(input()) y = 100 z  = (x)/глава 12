# 12.93
print("93:")
a = [[1,2,3],[4,5,6],[7,8,9]]
# Со списком
row_sums = [sum(row) for row in a]
col_sums = []
for j in range(len(a[0])):
    s = 0
    for i in range(len(a)):
        s += a[i][j]
    col_sums.append(s)
print("a со списком:", max(row_sums))
print("b со списком:", min(col_sums))

# Без списка
max_row_sum = sum(a[0])
for row in a:
    max_row_sum = max(max_row_sum, sum(row))
print("a без списка:", max_row_sum)

min_col_sum = 0
for j in range(len(a[0])):
    s = 0
    for i in range(len(a)):
        s += a[i][j]
    if j == 0:
        min_col_sum = s
    else:
        min_col_sum = min(min_col_sum, s)
print("b без списка:", min_col_sum)

