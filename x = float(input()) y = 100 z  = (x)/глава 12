    o_count = char_arr[i].count('о')
    if o_count == 3:
        count_3_o += 1
        print(f"278: строка {i} имеет ровно 3 буквы о")
print("278 всего строк с 3 буквами о:", count_3_o)

# 12.279
print("\n279:")
char_arr = [['и','г','р','а','и','и','и'],
            ['м','и','р','и',' ',' ',' '],
            ['с','и','л','а','и','и','и']]

for i in range(len(char_arr)):
    i_count = char_arr[i].count('и')
    if i_count == 4:
        word = ''.join(char_arr[i]).replace(' ', '')
        print(f"279 строка {i}: {word}")

# 12.280
print("\n280:")
char_arr = [['а',' ','б',' ','в'],
            [' ','г',' ','д',' '],
            ['е',' ',' ','ж',' ']]

max_spaces = 0
for row in char_arr:
    spaces = row.count(' ')
    max_spaces = max(max_spaces, spaces)
print("280 макс пробелов в строке:", max_spaces)

# 12.281
