# 12.190
print("\n190:")
n = 6
arr = [[(i*6 + j + 1) for j in range(n)] for i in range(n)]
print("Массив 6x6:")
for row in arr:
    print(row)

sum_top = sum_bottom = sum_left = sum_right = 0
for i in range(n):
    for j in range(n):
        if i != j and i + j != n-1:  # не на диагоналях
            if i < j and i + j < n-1:  # верхняя четверть
                sum_top += arr[i][j]
            elif i > j and i + j > n-1:  # нижняя четверть
                sum_bottom += arr[i][j]
            elif i > j and i + j < n-1:  # левая четверть
                sum_left += arr[i][j]
            elif i < j and i + j > n-1:  # правая четверть
                sum_right += arr[i][j]

print(f"a: сумма верхней четверти = {sum_top}")
print(f"b: сумма правой четверти = {sum_right}")
print(f"c: сумма нижней четверти = {sum_bottom}")
print(f"d: сумма левой четверти = {sum_left}")

