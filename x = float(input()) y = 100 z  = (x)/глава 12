    arr[i].append(0)  # добавляем пустой столбец
    for j in range(len(arr[i])-1, k, -1):
        arr[i][j] = arr[i][j-1]
    arr[i][k] = 0
print("241b:", arr)

# 12.242
print("\n242:")
arr = [[1,2,3],[4,5,6],[7,8,9]]
s = 1  # перед строкой с индексом 1
k = 1  # после столбца с индексом 1

# а) - имитация ввода с клавиатуры
