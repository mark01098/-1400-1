import random

# 12.141
print("141:")
a = [[1,2,3],[4,5,6],[7,8,9]]
# а)
row_num = 0
is_sorted = True
break_point = -1
for j in range(len(a[row_num])-1):
    if a[row_num][j] > a[row_num][j+1]:
        is_sorted = False
        break_point = j+1
        break
print("a:", is_sorted)
if not is_sorted:
    print(f"  нарушение на позиции ({row_num}, {break_point})")

# б)
col_num = 2
is_sorted = True
break_point = -1
for i in range(len(a)-1):
    if a[i][col_num] < a[i+1][col_num]:
        is_sorted = False
        break_point = i+1
        break
print("b:", is_sorted)
if not is_sorted:
    print(f"  нарушение на позиции ({break_point}, {col_num})")

