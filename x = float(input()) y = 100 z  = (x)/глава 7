first = True
max_sum = 0
min_product = 0

for i in range(n):
    a = int(input(f"Пара {i+1}, первое число: "))
    b = int(input(f"Пара {i+1}, второе число: "))
    
    pair_sum = a + b
    pair_product = a * b
    
    if first:
        max_sum = pair_sum
        min_product = pair_product
        first = False
    else:
        if pair_sum > max_sum:
            max_sum = pair_sum
        if pair_product < min_product:
            min_product = pair_product

print(f"Максимальная сумма в паре: {max_sum}")
print(f"Минимальное произведение в паре: {min_product}")

#  7.59 
n = int(input("Введите количество пар n: "))
print(f"Введите {n} пар положительных чисел:")

first = True
max_avg = 0
max_avg_index = 0
min_geom = float('inf')
min_geom_index = 0

for i in range(1, n + 1):
    a = float(input(f"Пара {i}, первое число: "))
    b = float(input(f"Пара {i}, второе число: "))
    
    avg = (a + b) / 2
    geom = (a * b) ** 0.5  
    
    if first:
        max_avg = avg
        max_avg_index = i
        min_geom = geom
        min_geom_index = i
        first = False
    else:
        if avg >= max_avg:  
            max_avg = avg
            max_avg_index = i
        if geom < min_geom: 
            min_geom = geom
            min_geom_index = i

print(f"Пара {max_avg_index} имеет максимальное среднее арифметическое: {max_avg:.2f}")
print(f"Пара {min_geom_index} имеет минимальное среднее геометрическое: {min_geom:.2f}")

