# 7.15
sum_odd_pos = 0
print("Введите 15 вещественных чисел:")
for i in range(1, 16):
    num = float(input(f"Число {i}: "))
    if i % 2 == 1: 
        sum_odd_pos -= num
print(f"Сумма -c1 - c3 - c5 - ...: {sum_odd_pos}")

# 7.16
n = int(input("Введите количество чисел n: "))
print(f"Введите {n} целых чисел:")
sum_alt = 0
sign = 1
for i in range(n):
    num = int(input(f"Число {i+1}: "))
    sum_alt += sign * num
    sign *= -1
print(f"a1 - a2 + a3 - ...: {sum_alt}")
print("Введите те же числа ещё раз:")
first = int(input("Первое число: "))
for i in range(n-1):
    num = int(input(f"Число {i+2}: "))
last = num 
print(f"a1 + an: {first + last}")


print(f"a1 - a2: {first - last}")  
# 7.17
total = 0
print("Вводите стоимости товаров (0 для окончания):")
while True:
    price = float(input("Стоимость: "))
    if price == 0:
        break
    if price > 1000:
        total += price
print(f"Общая стоимость товаров дороже 1000 руб: {total}")

# 7.18
total_pages = 0
print("Вводите количество страниц (0 для окончания):")
while True:
    pages = int(input("Страниц: "))
    if pages == 0:
        break
    if pages > 16:  
        total_pages += pages
print(f"Общее число страниц во всех журналах: {total_pages}")

# 7.19

rain_even_days = 0
print("Введите количество осадков за каждый день месяца:")
for day in range(1, 32):
    rain = int(input(f"День {day}: "))
    if day % 2 == 0: 
        rain_even_days += rain
print(f"Общее количество осадков в чётные дни: {rain_even_days}")

