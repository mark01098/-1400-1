print(f"Введите {n} целых чисел:")


first = int(input("Число 1: "))
second = int(input("Число 2: "))
third = int(input("Число 3: "))


if first >= second and first >= third:
    max1 = first
    if second >= third:
        max2 = second
        min1 = third
        min2 = second
    else:
        max2 = third
        min1 = second
        min2 = third
elif second >= first and second >= third:
    max1 = second
    if first >= third:
        max2 = first
        min1 = third
        min2 = first
    else:
        max2 = third
        min1 = first
        min2 = third
else: 
    max1 = third
    if first >= second:
        max2 = first
        min1 = second
        min2 = first
    else:
        max2 = second
        min1 = first
        min2 = second


for i in range(4, n + 1):
    num = int(input(f"Число {i}: "))
    
    if num > max1:
        max2 = max1
        max1 = num
    elif num > max2:
        max2 = num
    
    if num < min1:
        min2 = min1
        min1 = num
    elif num < min2:
        min2 = num

print(f"Два максимальных: {max1}, {max2}")
print(f"Два минимальных: {min1}, {min2}")

