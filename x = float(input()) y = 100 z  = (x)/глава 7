current_length = 0
in_zero_segment = False

for i in range(m):
    num = int(input(f"Число {i+1} (0 или 1): "))
    
    if num == 0:
        current_length += 1
        in_zero_segment = True
    else:  
        if in_zero_segment:
            if current_length < min_length:
                min_length = current_length
            current_length = 0
            in_zero_segment = False

if in_zero_segment and current_length < min_length:
    min_length = current_length

if min_length == float('inf'):
    print("Нулевых отрезков нет")
else:
    print(f"Наименьшая длина отрезка из нулей: {min_length}")

#  7.71

n = int(input("Введите количество покупателей n: "))
print(f"Введите время обслуживания каждого покупателя:")

waiting_times = []
service_times = []
min_service_time = float('inf')
min_service_customer = 0

for i in range(1, n + 1):
    t = float(input(f"Покупатель {i}: "))
    service_times.append(t)
    
    if t < min_service_time:
        min_service_time = t
        min_service_customer = i
    
    
    waiting_time = sum(service_times) - t  
    waiting_times.append(waiting_time)

print("\nВремя пребывания в очереди:")
for i in range(n):
    print(f"Покупатель {i+1}: {waiting_times[i]:.1f} мин")

print(f"\nМеньше всего времени потребовалось для обслуживания покупателя {min_service_customer}: {min_service_time:.1f} мин")

#  7.72

n = int(input("Введите количество чисел n (n ≥ 3): "))
print(f"Введите {n} целых чисел:")

prev = int(input("Число 1: "))
current = int(input("Число 2: "))

max_sum = prev + current
min_sum = prev + current
max_pair = (1, 2)  
min_pair = (1, 2)

for i in range(3, n + 1):
    next_num = int(input(f"Число {i}: "))
    current_sum = current + next_num
    
    if current_sum > max_sum:
        max_sum = current_sum
        max_pair = (i-1, i)
    
    if current_sum < min_sum:
        min_sum = current_sum
        min_pair = (i-1, i)
    
    current = next_num

print(f"Максимальная сумма двух соседних: {max_sum} (числа {max_pair[0]} и {max_pair[1]})")
print(f"Минимальная сумма двух соседних: {min_sum} (числа {min_pair[0]} и {min_pair[1]})")

