print("Введите очки 20 команд:")
scores = []
for i in range(20):
    score = int(input(f"Команда {i+1}: "))
    scores.append(score)

if scores[0] >= scores[1]:
    max1 = scores[0]
    max2 = scores[1]
else:
    max1 = scores[1]
    max2 = scores[0]

for i in range(2, 20):
    if scores[i] > max1:
        max2 = max1
        max1 = scores[i]
    elif scores[i] > max2:
        max2 = scores[i]

print(f"Сумма очков команд, занявших первые два места: {max1 + max2}")

#  7.74 

n = int(input("Введите количество спортсменов n (n ≥ 4): "))
print("Введите результаты в сотых долях секунды:")

results = []
for i in range(n):
    time = float(input(f"Спортсмен {i+1}: "))
    results.append(time)

indexed_results = list(enumerate(results))
indexed_results.sort(key=lambda x: x[1])  
best_four = indexed_results[:4]
best_indices = [idx + 1 for idx, _ in best_four]  
best_times = [time for _, time in best_four]

print(f"Лучшая команда из спортсменов: {best_indices}")
print(f"Сумма их результатов: {sum(best_times):.2f}")

#  7.75

print("Введите результаты 20 игр в формате 'забито-пропущено':")

wins = draws = losses = 0
total_points = 0
big_difference_games = 0

for i in range(1, 21):
    result = input(f"Игра {i}: ")
    scored, conceded = map(int, result.split('-'))
    
    if scored > conceded:
        wins += 1
        total_points += 3
        print(f"Игра {i}: выигрыш")
    elif scored == conceded:
        draws += 1
        total_points += 1
        print(f"Игра {i}: ничья")
    else:
        losses += 1
        print(f"Игра {i}: проигрыш")
    
    difference = abs(scored - conceded)
    if difference >= 3:
        big_difference_games += 1

print(f"\nВыигрышей: {wins}")
print(f"Ничьих: {draws}")
print(f"Проигрышей: {losses}")
print(f"Всего очков: {total_points}")
print(f"Игр с разницей ≥3 мячей: {big_difference_games}")

