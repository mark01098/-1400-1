print(f"Ничьих: {draws}")
print(f"Проигрышей: {losses}")

#  7.39 

print("Введите 9 вещественных чисел:")
sum_greater_10 = 0
count_greater_10 = 0
for i in range(9):
    num = float(input(f"Число {i+1}: "))
    if num > 10:
        sum_greater_10 += num
        count_greater_10 += 1

