# 7.24
n = int(input("Введите количество чисел n: "))
print(f"Введите {n} целых чисел:")
p = int(input("Введите p: "))
count_gt_p = 0
count_ends_5 = 0
k = int(input("Введите k: "))
count_mult_k = 0

for i in range(n):
    num = int(input(f"Число {i+1}: "))
    if num > p:
        count_gt_p += 1
    if num % 10 == 5:
        count_ends_5 += 1
    if num % k == 0:
        count_mult_k += 1

print(f"Чисел >{p}: {count_gt_p}")
print(f"Чисел, оканчивающихся на 5: {count_ends_5}")
print(f"Чисел, кратных {k}: {count_mult_k}")

