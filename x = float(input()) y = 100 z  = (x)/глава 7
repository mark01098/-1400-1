#  7.101
print("Введите стоимости автомобилей (0 для окончания):")
car_prices = []
while True:
    price = float(input("Стоимость автомобиля: "))
    if price == 0:
        break
    car_prices.append(price)

print("Введите стоимости мотоциклов (0 для окончания):")
motorcycle_prices = []
while True:
    price = float(input("Стоимость мотоцикла: "))
    if price == 0:
        break
    motorcycle_prices.append(price)

if car_prices and motorcycle_prices:
    avg_car = sum(car_prices) / len(car_prices)
    avg_motorcycle = sum(motorcycle_prices) / len(motorcycle_prices)
    
    if avg_car > avg_motorcycle * 3:
        print(f"Средняя стоимость автомобилей ({avg_car:.2f}) превышает среднюю стоимость мотоциклов ({avg_motorcycle:.2f}) более чем в 3 раза: ДА")
    else:
        print(f"Средняя стоимость автомобилей ({avg_car:.2f}) не превышает среднюю стоимость мотоциклов ({avg_motorcycle:.2f}) более чем в 3 раза: НЕТ")
else:
    print("Недостаточно данных")

# 7.102

print("Введите рост каждого ученика (отрицательные - мальчики, положительные - девочки):")
boys_heights = []
girls_heights = []

for i in range(30):  
    height = float(input(f"Ученик {i+1}: "))
    if height < 0:
        boys_heights.append(-height)  
    else:
        girls_heights.append(height)

if boys_heights and girls_heights:
    avg_boy = sum(boys_heights) / len(boys_heights)
    avg_girl = sum(girls_heights) / len(girls_heights)
    
    if avg_boy > avg_girl + 10:
        print(f"Средний рост мальчиков ({avg_boy:.1f}) превышает средний рост девочек ({avg_girl:.1f}) более чем на 10 см: ДА")
    else:
        print(f"Средний рост мальчиков ({avg_boy:.1f}) не превышает средний рост девочек ({avg_girl:.1f}) более чем на 10 см: НЕТ")
else:
    print("Недостаточно данных")

