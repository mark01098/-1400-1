athlete2_total = 0
for i in range(1, 11):
    points = int(input(f"Вид {i}: "))
    athlete2_total += points

if athlete1_total > athlete2_total:
    print(f"Первый спортсмен показал лучший результат: {athlete1_total} > {athlete2_total}")
elif athlete2_total > athlete1_total:
    print(f"Второй спортсмен показал лучший результат: {athlete2_total} > {athlete1_total}")
else:
    print(f"Результаты равны: {athlete1_total} = {athlete2_total}")

