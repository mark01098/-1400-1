print("Введите массу каждого человека (0 для окончания):")
first = True
max_weight = 0
min_weight = 0

while True:
    weight = float(input("Масса: "))
    if weight == 0:
        break
    
    if first:
        max_weight = weight
        min_weight = weight
        first = False
    else:
        if weight > max_weight:
            max_weight = weight
        if weight < min_weight:
            min_weight = weight

if min_weight > 0:  
    ratio = max_weight / min_weight
    if ratio > 2:
        print(f"Самый тяжелый ({max_weight}) тяжелее самого лёгкого ({min_weight}) в {ratio:.1f} раз >2: ДА")
    else:
        print(f"Самый тяжелый ({max_weight}) не тяжелее самого лёгкого ({min_weight}) в 2 раза: НЕТ")
else:
    print("Ошибка: минимальный вес 0")

