print("Введите возраст людей (0 для окончания):")
first = True
max_age = 0
min_age = 0
max_first_index = 0
min_first_index = 0
person = 1

while True:
    age = int(input(f"Человек {person}: "))
    if age == 0:
        break
    
    if first:
        max_age = age
        min_age = age
        max_first_index = person
        min_first_index = person
        first = False
    else:
        if age > max_age:
            max_age = age
            max_first_index = person
        if age < min_age:
            min_age = age
            min_first_index = person
    
    person += 1

if max_first_index < min_first_index:
    print(f"Самый старший указан раньше (номер {max_first_index})")
elif min_first_index < max_first_index:
    print(f"Самый молодой указан раньше (номер {min_first_index})")
else:
    print(f"Самый старший и самый молодой - один человек (номер {max_first_index})")

# 7.113 
print("Введите результаты на этапах (время в минутах, 0 для окончания):")
best_time = float('inf')
worst_time = 0
best_stage = 0
worst_stage = 0
stage = 1

while True:
    time = float(input(f"Этап {stage}: "))
    if time == 0:
        break
    
    if time < best_time:
        best_time = time
        best_stage = stage
    
    if time > worst_time:
        worst_time = time
        worst_stage = stage
    
    stage += 1

if best_stage < worst_stage:
    print(f"Этап с первым местом (№{best_stage}) был раньше этапа с последним местом (№{worst_stage}): ДА")
else:
    print(f"Этап с первым местом (№{best_stage}) был не раньше этапа с последним местом (№{worst_stage}): НЕТ")

#  7.114

print("Введите 20 чисел в неубывающем порядке:")
prev = None
count_equal = 1
total_equal = 0
different = 0

for i in range(20):
    num = int(input(f"Число {i+1}: "))
    
    if prev is None:
        prev = num
        different = 1
    else:
        if num == prev:
            count_equal += 1
        else:
            if count_equal > 1:
                total_equal += count_equal
            count_equal = 1
            different += 1
            prev = num


if count_equal > 1:
    total_equal += count_equal

print(f"Чисел, равных подряд: {total_equal}")
print(f"Разных чисел: {different}")

#  7.115 

print("Введите 30 чисел в неубывающем порядке:")
prev = None
different = 0

for i in range(30):
    num = int(input(f"Число {i+1}: "))
    
    if prev is None or num != prev:
        different += 1
        prev = num

print(f"Разных чисел: {different}")

#  7.116 

print("Введите 20 чисел от 0 до 66:")
prev_right = None  
is_domino = True


print("Случай а) последняя цифра = правая половина:")
for i in range(20):
    num = int(input(f"Число {i+1}: "))
    right = num % 10  
    
    if prev_right is not None and right != prev_right:
        is_domino = False
    
    prev_right = num // 10 

if is_domino:
    print("Последовательность соответствует костям домино: ДА")
else:
    print("Последовательность не соответствует костям домино: НЕТ")


print("\nСлучай б) любая цифра может соответствовать любой половине:")
prev_digits = None
is_domino = True

for i in range(20):
    num = int(input(f"Число {i+1}: "))
    d1 = num // 10
    d2 = num % 10
    
    if prev_digits is not None:
        if not (d1 in prev_digits or d2 in prev_digits):
            is_domino = False
    
    prev_digits = (d1, d2)

if is_domino:
    print("Последовательность соответствует костям домино: ДА")
else:
    print("Последовательность не соответствует костям домино: НЕТ")

#  7.117

n = int(input("Введите натуральное число: "))
is_prime = True

if n <= 1:
    is_prime = False
else:
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            is_prime = False
            break

if is_prime:
    print("Число простое: ДА")
else:
    print("Число простое: НЕТ")

#  7.118 
n = int(input("Введите натуральное число: "))
sum_divisors = 0

for i in range(1, n):
    if n % i == 0:
        sum_divisors += i

if sum_divisors == n:
    print(f"Число {n} совершенное: ДА")
else:
    print(f"Число {n} не совершенное: НЕТ (сумма делителей = {sum_divisors})")


