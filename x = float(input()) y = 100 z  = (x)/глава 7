print("Введите данные о 30 телах:")
max_density = 0

for i in range(30):
    mass = float(input(f"Тело {i+1}, масса (кг): "))
    volume = float(input(f"Тело {i+1}, объем (см³): "))
    
    
    volume_m3 = volume / 1000000
    density = mass / volume_m3
    
    if density > max_density:
        max_density = density

print(f"Максимальная плотность: {max_density:.2f} кг/м³")

