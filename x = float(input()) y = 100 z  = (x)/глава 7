# 7.26

count_before_1985 = 0
count_after_1990 = 0
print("Вводите годы рождения (0 для окончания):")
while True:
    year = int(input("Год рождения: "))
    if year == 0:
        break
    if year < 1985:
        count_before_1985 += 1
    elif year > 1990:
        count_after_1990 += 1
print(f"Родившихся до 1985: {count_before_1985}")
print(f"Родившихся после 1990: {count_after_1990}")

