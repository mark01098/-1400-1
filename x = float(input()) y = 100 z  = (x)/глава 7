    num = int(input(f"Число {i}: "))
    
    if first:
        max_num = num
        min_num = num
        max_first_index = i
        min_first_index = i
        first = False
    else:
        if num > max_num:
            max_num = num
            max_first_index = i
        if num < min_num:
            min_num = num
            min_first_index = i

if max_first_index < min_first_index:
    print(f"Максимальное число встретилось раньше (номер {max_first_index})")
elif min_first_index < max_first_index:
    print(f"Минимальное число встретилось раньше (номер {min_first_index})")
