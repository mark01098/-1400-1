#  7.43

print("Введите массу людей в группе:")
sum_full = 0
count_full = 0
sum_others = 0
count_others = 0

while True:
    weight = float(input("Масса (0 для окончания): "))
    if weight == 0:
        break
    if weight > 100:
        sum_full += weight
        count_full += 1
    else:
        sum_others += weight
        count_others += 1

if count_full > 0:
    avg_full = sum_full / count_full
    print(f"Средняя масса полных людей: {avg_full:.1f} кг")
else:
    print("Полных людей нет")

if count_others > 0:
    avg_others = sum_others / count_others
    print(f"Средняя масса остальных: {avg_others:.1f} кг")
else:
    print("Остальных людей нет")

#  7.44 

n = int(input("Введите количество чисел n: "))
print(f"Введите {n} вещественных чисел:")



first = True
max_num = 0
min_num = 0

for i in range(n):
    num = float(input(f"Число {i+1}: "))
    if first:
        max_num = num
        min_num = num
        first = False
    else:
        if num > max_num:
            max_num = num
        if num < min_num:
            min_num = num

print(f"Максимальное: {max_num}")
print(f"Минимальное: {min_num}")

#  7.45 

n = int(input("Введите количество чисел n: "))
print(f"Введите {n} целых чисел:")

first = True
max_num = 0
min_num = 0
max_index = 0
min_index = 0

for i in range(n):
    num = int(input(f"Число {i+1}: "))
    if first:
        max_num = num
        min_num = num
        max_index = i + 1
        min_index = i + 1
        first = False
    else:
        if num >= max_num: 
            max_num = num
            max_index = i + 1
        if num < min_num:  
            min_num = num
            min_index = i + 1

print(f"Номер последнего максимального: {max_index}")
print(f"Номер первого минимального: {min_index}")

#  7.46 

print("Введите результаты спортсменов (время в минутах):")
best_time = float('inf')  
while True:
    time = float(input("Результат (0 для окончания): "))
    if time == 0:
        break
    if time < best_time:
        best_time = time
        print(f"Новый лучший результат: {best_time} мин")

print(f"Окончательный лучший результат: {best_time} мин")

#  7.47

print("Введите расстояния от Москвы до городов:")
max_distance = 0

while True:
    distance = float(input("Расстояние (0 для окончания): "))
    if distance == 0:
        break
    if distance > max_distance:
        max_distance = distance

