    num = int(input(f"Число {i+1}: "))
    if first:
        max_num = num
        min_num = num
        first = False
    else:
        if num > max_num:
            max_num = num
        if num < min_num:
            min_num = num

difference = max_num - min_num
if difference <= 25:
    print(f"Максимальное ({max_num}) превышает минимальное ({min_num}) на {difference} ≤25: ДА")
else:
    print(f"Максимальное ({max_num}) превышает минимальное ({min_num}) на {difference} >25: НЕТ")

