# 7.31
n = int(input("Введите количество чисел n: "))
print(f"Введите {n} целых чисел:")
count_equal_pairs = 0
count_zero_pairs = 0
count_even_pairs = 0
count_ends5_pairs = 0

prev = None
for i in range(n):
    num = int(input(f"Число {i+1}: "))
    if prev is not None:
        if num == prev:
            count_equal_pairs += 1
        if num == 0 and prev == 0:
            count_zero_pairs += 1
        if num % 2 == 0 and prev % 2 == 0:
            count_even_pairs += 1
        if num % 10 == 5 and prev % 10 == 5:
            count_ends5_pairs += 1
    prev = num

print(f"Пар равных соседних чисел: {count_equal_pairs}")
print(f"Пар нулевых соседних чисел: {count_zero_pairs}")
print(f"Пар чётных соседних чисел: {count_even_pairs}")
print(f"Пар, оканчивающихся на 5: {count_ends5_pairs}")

# 7.32

n = int(input("Введите количество чисел n: "))
print(f"Введите {n} вещественных чисел:")
count_greater_than_neighbors = 0
nums = []
for i in range(n):
    num = float(input(f"Число {i+1}: "))
    nums.append(num)

for i in range(1, n-1):  
    if nums[i] > nums[i-1] and nums[i] > nums[i+1]:
        count_greater_than_neighbors += 1

print(f"Чисел, больших своих соседей: {count_greater_than_neighbors}")

# 7.33

print("Вводите ненулевые целые числа (0 для окончания):")
prev = None
count_changes = 0
while True:
    num = int(input("Число: "))
    if num == 0:
        break
    if prev is not None:
        if (prev > 0 and num < 0) or (prev < 0 and num > 0):
            count_changes += 1
    prev = num
print(f"Знак менялся {count_changes} раз")

# 7.34
n = int(input("Введите количество троек n: "))
count_triangles = 0
print(f"Введите {n} троек чисел (a ≤ b ≤ c):")
for i in range(n):
    a = int(input(f"Тройка {i+1}, a: "))
    b = int(input(f"Тройка {i+1}, b: "))
    c = int(input(f"Тройка {i+1}, c: "))
    if a + b > c:
        count_triangles += 1
print(f"Троек, из которых можно построить треугольник: {count_triangles}")

# 7.35
import math
n = int(input("Введите количество пар (α, v0) n: "))
count_hits = 0
print("Введите данные цели:")
P = float(input("Высота цели P: "))
R = float(input("Расстояние R: "))
H = float(input("Высота расположения цели H: "))
g = 9.8

print(f"Введите {n} пар значений α (в градусах) и v0:")
for i in range(n):
    alpha = float(input(f"α {i+1} (град): "))
    v0 = float(input(f"v0 {i+1}: "))
    
    alpha_rad = math.radians(alpha)
    
    t = R / (v0 * math.cos(alpha_rad))
    
    y = v0 * t * math.sin(alpha_rad) - g * t * t / 2
    
    if H <= y <= H + P:
        count_hits += 1

percentage = (count_hits / n) * 100
print(f"Процент попаданий: {percentage:.1f}%")

#  7.36

print("В хоккейном матче было 24 удаления.")
team1_total_time = 0
team2_total_time = 0
team1_count = 0
team2_count = 0

for i in range(24):
    team = int(input(f"Удаление {i+1}, команда (1/2): "))
    time = int(input("Длительность (2,5,10): "))
    
    if team == 1:
        team1_total_time += time
        team1_count += 1
    else:
        team2_total_time += time
        team2_count += 1

print(f"Команда 1: {team1_count} удалений, {team1_total_time} минут")
print(f"Команда 2: {team2_count} удалений, {team2_total_time} минут")

