    set2_total += price

if set1_total < set2_total:
    print(f"Первый набор дешевле: {set1_total} < {set2_total}")
elif set2_total < set1_total:
    print(f"Второй набор дешевле: {set2_total} < {set1_total}")
else:
    print(f"Наборы стоят одинаково: {set1_total} = {set2_total}")

