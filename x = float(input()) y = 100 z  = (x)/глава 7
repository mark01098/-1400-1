for i in range(12):
    grade = int(input(f"Оценка {i+1}: "))
    if grade == 3:
        has_3 = True

if not has_3:
    print("Среди оценок нет троек: ДА")
else:
    print("Среди оценок есть тройки: НЕТ")

print("Проверка с циклом while:")
i = 0
has_3 = False
while i < 12 and not has_3:
    grade = int(input(f"Оценка {i+1}: "))
    if grade == 3:
        has_3 = True
    i += 1

if not has_3:
    print("Среди оценок нет троек: ДА")
else:
    print("Среди оценок есть тройки: НЕТ")

