# 7.1
print("Четырёхзначные числа, которые при делении на 133 дают остаток 125,")
print("а при делении на 134 дают остаток 111:")
for num in range(1000, 10000):
    if num % 133 == 125 and num % 134 == 111:
        print(num, end=' ')

# 7.2
count = 0
for num in range(100, 501):
    s = 0
    temp = num
    while temp > 0:
        s += temp % 10
        temp //= 10
    if s == 15:
        count += 1
print(f"\nКоличество чисел от 100 до 500 с суммой цифр 15: {count}")

# 7.3
n = int(input("Введите n (0 < n ≤ 27): "))
count = 0
for num in range(100, 1000):
    s = num//100 + (num//10)%10 + num%10
    if s == n:
        count += 1
print(f"Количество трёхзначных чисел с суммой цифр {n}: {count}")

# 7.4
print("Двузначные числа, сумма квадратов цифр делится на 13:")
for num in range(10, 100):
    d1 = num // 10
    d2 = num % 10
    if (d1*d1 + d2*d2) % 13 == 0:
        print(num, end=' ')

print("\nДвузначные числа с особым свойством:")
for num in range(10, 100):
    s = num//10 + num%10
    if s + s*s == num:
        print(num, end=' ')

# 7.5
n = int(input("Введите цифру n (0-9): "))
print(f"Двузначные числа, делящиеся на {n} или содержащие цифру {n}:")
for num in range(10, 100):
    if num % n == 0 or num//10 == n or num%10 == n:
        print(num, end=' ')

# 7.6
print("Трёхзначные числа, квадрат оканчивается на те же цифры:")
for num in range(100, 1000):
    square = num * num
    if square % 1000 == num:
        print(num, end=' ')

print("\nТрёхзначные числа, кратные 7, сумма цифр кратна 7:")
for num in range(100, 1000):
    if num % 7 == 0:
        s = num//100 + (num//10)%10 + num%10
        if s % 7 == 0:
            print(num, end=' ')

# 7.7
sum_num = 0
for num in range(30, 100):
    if num % 3 == 0 and (num % 10 in [2, 4, 8]):
        sum_num += num
print(f"Сумма чисел от 30 до 100, кратных 3 и оканчивающихся на 2,4,8: {sum_num}")

# 7.8
n = int(input("Введите натуральное число: "))
print("Делители числа:", end=' ')
for i in range(1, n+1):
    if n % i == 0:
        print(i, end=' ')

sum_div = 0
for i in range(1, n+1):
    if n % i == 0:
        sum_div += i
print(f"\nСумма делителей: {sum_div}")

sum_even = 0
for i in range(1, n+1):
    if n % i == 0 and i % 2 == 0:
        sum_even += i
print(f"Сумма чётных делителей: {sum_even}")

count_div = 0
for i in range(1, n+1):
    if n % i == 0:
        count_div += 1
print(f"Количество делителей: {count_div}")

count_odd = 0
for i in range(1, n+1):
    if n % i == 0 and i % 2 == 1:
        count_odd += 1
print(f"Количество нечётных делителей: {count_odd}")

count_even_div = 0
for i in range(1, n+1):
    if n % i == 0:
        if i % 2 == 0:
            count_even_div += 1
print(f"Всего делителей: {count_div}, из них чётных: {count_even_div}")

d = int(input("Введите d: "))
count_greater = 0
for i in range(1, n+1):
    if n % i == 0 and i > d:
        count_greater += 1
print(f"Количество делителей > {d}: {count_greater}")

