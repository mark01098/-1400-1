print("Введите площади нескольких кругов:")
min_radius = float('inf')

while True:
    area = float(input("Площадь (0 для окончания): "))
    if area == 0:
        break
    
    import math
    radius = math.sqrt(area / math.pi)
    if radius < min_radius:
        min_radius = radius

print(f"Радиус самого маленького круга: {min_radius:.2f}")

#  7.50 

print("Введите площади нескольких квадратов:")
max_diagonal = 0

while True:
    area = float(input("Площадь (0 для окончания): "))
    if area == 0:
        break
    
    import math
    side = math.sqrt(area)
    diagonal = side * math.sqrt(2)
    if diagonal > max_diagonal:
        max_diagonal = diagonal

print(f"Диагональ самого большого квадрата: {max_diagonal:.2f}")

