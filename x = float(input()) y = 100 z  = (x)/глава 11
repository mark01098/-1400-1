# 11.153
print("11.153")
arr = [15, -5, 25, 8, -30, 12, 18, -7, 22]

neg_idx = -1
for i in range(len(arr)):
    if arr[i] < 0:
        neg_idx = i
        break

if neg_idx != -1:
    for i in range(neg_idx, len(arr)-1):
        arr[i] = arr[i+1]
    arr[-1] = 0

