# 11.106
print("11.106")
arr = [1, 3, 5, 2, 7, 9, 11, 4, 13, 15, 17, 6, 19, 21, 23]
max_len = 0
curr_len = 0
for x in arr:
    if x % 2 != 0:
        curr_len += 1
        if curr_len > max_len:
            max_len = curr_len
    else:
        curr_len = 0

# 11.107
