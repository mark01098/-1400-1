# 11.106
arr = [1, 3, 5, 2, 7, 9, 11, 4, 13, 15, 17, 6, 19, 21, 23]
max_len = 0
curr_len = 0
for x in arr:
    if x % 2 != 0:  # нечетное
        curr_len += 1
        if curr_len > max_len:
            max_len = curr_len
    else:  # четное - прерывает последовательность
        curr_len = 0

# 11.107
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22]

# а) максимальный элемент
max_val = arr[0]
for x in arr:
    if x > max_val:
        max_val = x

# б) минимальный элемент
min_val = arr[0]
for x in arr:
    if x < min_val:
        min_val = x

# в) на сколько максимальный больше минимального
diff = max_val - min_val

# г) индекс максимального элемента
max_idx = 0
for i in range(1, len(arr)):
    if arr[i] > arr[max_idx]:
        max_idx = i

# д) индекс минимального и максимального элементов
min_idx = 0
max_idx = 0
for i in range(1, len(arr)):
    if arr[i] > arr[max_idx]:
        max_idx = i
    if arr[i] < arr[min_idx]:
        min_idx = i

