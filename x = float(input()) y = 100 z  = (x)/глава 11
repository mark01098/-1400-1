# 11.113
birth_years = [1990, 1985, 1995, 1980, 1992, 1988, 1998, 1975, 1993, 1987]
oldest = birth_years[0]  # самый старший - с наименьшим годом
youngest = birth_years[0]  # самый молодой - с наибольшим годом
for y in birth_years:
    if y < oldest:  # чем меньше год, тем старше
        oldest = y
    if y > youngest:  # чем больше год, тем моложе
        youngest = y
age_diff = youngest - oldest  # разница в годах рождения

# 11.114
scores = [5.8, 6.0, 5.5, 5.9, 6.1, 5.7, 5.8, 6.0]

# Удаляем одну максимальную и одну минимальную оценку
max_score = max(scores)
min_score = min(scores)

# Флаги для удаления только по одной оценке каждого типа
max_removed = False
min_removed = False

total = 0
count = 0
for s in scores:
    if not max_removed and s == max_score:
        max_removed = True
    elif not min_removed and s == min_score:
        min_removed = True
    else:
        total += s
        count += 1

if count > 0:
    final_score = total / count

# 11.115
speeds = [180, 200, 190, 210, 200, 220, 210, 230, 220, 200]
max_speed = speeds[0]

# Находим максимальную скорость
for s in speeds:
    if s > max_speed:
        max_speed = s

# а) индекс первого самого быстрого
first_idx = 0
for i in range(len(speeds)):
    if speeds[i] == max_speed:
        first_idx = i
        break

# б) индекс последнего самого быстрого
last_idx = 0
for i in range(len(speeds)):
    if speeds[i] == max_speed:
        last_idx = i

