for i in range(len(arr)):
    if arr[i] < 0:
        first_neg_idx = i
        break

# Находим последний положительный
last_pos_idx = -1
for i in range(len(arr)-1, -1, -1):
    if arr[i] > 0:
        last_pos_idx = i
        break

# Меняем местами, если оба найдены
if first_neg_idx != -1 and last_pos_idx != -1:
    arr[first_neg_idx], arr[last_pos_idx] = arr[last_pos_idx], arr[first_neg_idx]

