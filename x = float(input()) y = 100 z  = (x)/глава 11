# 11.216
areas = [1200, 1500, 800, 2000, 1700, 1900, 1600, 2100, 1400, 1300,
         2200, 1800, 2000, 1600, 1900, 1700, 2100, 1500, 2300, 1400]

harvests = [4800, 6750, 3200, 9000, 7650, 8550, 7200, 9450, 6300, 5850,
            9900, 8100, 9000, 7200, 8550, 7650, 9450, 6750, 10350, 6300]

# Способ 1: без дополнительного массива
district_yields = []
for i in range(len(areas)):
    yield_per_hectare = harvests[i] / areas[i] if areas[i] > 0 else 0
    district_yields.append(yield_per_hectare)

total_area = sum(areas)
total_harvest = sum(harvests)
overall_yield = total_harvest / total_area if total_area > 0 else 0

# Способ 2: с дополнительным массивом
yields_array = [0] * len(areas)
for i in range(len(areas)):
    yields_array[i] = harvests[i] / areas[i] if areas[i] > 0 else 0

