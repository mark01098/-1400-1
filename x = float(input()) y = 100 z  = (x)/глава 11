# 11.147
arr = list(range(1, 16))  # [1, 2, 3, ..., 15]

# а) перевернуть элементы между 2-м и 10-м (индексы 1 и 9)
start = 2  # индекс третьего элемента
end = 9    # индекс девятого элемента
while start < end:
    arr[start], arr[end] = arr[end], arr[start]
    start += 1
    end -= 1

# б) перевернуть элементы между k-м и s-м
arr = list(range(1, 16))  # восстановим
k = 3  # например
s = 10 # например
start = k  # k-й элемент имеет индекс k-1, следующий - индекс k
end = s-2  # s-й элемент имеет индекс s-1, предыдущий - s-2
while start < end:
    arr[start], arr[end] = arr[end], arr[start]
    start += 1
    end -= 1

# в) перевернуть элементы между максимальным и минимальным
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22, 3, 28, 14, 20, 9, 17]
# Находим индексы максимума и минимума
max_idx = 0
min_idx = 0
for i in range(1, len(arr)):
    if arr[i] > arr[max_idx]:
        max_idx = i
    if arr[i] < arr[min_idx]:
        min_idx = i

# Определяем начало и конец диапазона
start = min(max_idx, min_idx)
end = max(max_idx, min_idx)

# Переворачиваем элементы между ними включительно
while start < end:
    arr[start], arr[end] = arr[end], arr[start]
    start += 1
    end -= 1

# 11.148
arr = [15, -5, 25, 8, -30, 12, 18, -7, 22]
