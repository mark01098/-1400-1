for i in range(len(arr)):
    if not (arr[i] % 3 == 0 or arr[i] % 5 == 0):
        arr[write_idx] = arr[i]
        write_idx += 1

for i in range(write_idx, len(arr)):
    arr[i] = 0

# 11.158
arr = [15, 5, 25, 8, 15, 30, 5, 12, 25, 18, 7, 22, 8, 30, 15]
# Используем дополнительный список для уникальных элементов
unique = []
for x in arr:
    if x not in unique:
        unique.append(x)

# Копируем обратно в исходный массив
for i in range(len(arr)):
    if i < len(unique):
        arr[i] = unique[i]
    else:
        arr[i] = 0

