# 11.147
print("11.147")
arr = list(range(1, 16))

start = 2
end = 9
while start < end:
    arr[start], arr[end] = arr[end], arr[start]
    start += 1
    end -= 1

arr = list(range(1, 16))
k = 3
s = 10
start = k
end = s-2
while start < end:
    arr[start], arr[end] = arr[end], arr[start]
    start += 1
    end -= 1

arr = [15, 5, 25, 8, 30, 12, 18, 7, 22, 3, 28, 14, 20, 9, 17]
max_idx = 0
min_idx = 0
for i in range(1, len(arr)):
    if arr[i] > arr[max_idx]:
        max_idx = i
    if arr[i] < arr[min_idx]:
        min_idx = i

start = min(max_idx, min_idx)
end = max(max_idx, min_idx)
while start < end:
    arr[start], arr[end] = arr[end], arr[start]
    start += 1
    end -= 1

# 11.148
print("11.148")
arr = [15, -5, 25, 8, -30, 12, 18, -7, 22]
first_neg_idx = -1
for i in range(len(arr)):
    if arr[i] < 0:
        first_neg_idx = i
        break

last_pos_idx = -1
for i in range(len(arr)-1, -1, -1):
    if arr[i] > 0:
        last_pos_idx = i
        break

if first_neg_idx != -1 and last_pos_idx != -1:
    arr[first_neg_idx], arr[last_pos_idx] = arr[last_pos_idx], arr[first_neg_idx]

