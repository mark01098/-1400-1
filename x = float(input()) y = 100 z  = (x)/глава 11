# 11.59
arr = [1.5, -2.3, 4.7, -1.2, 3.8, -0.5]
sp = 0  # сумма положительных
sn = 0  # сумма отрицательных
for x in arr:
    if x > 0:
        sp += x
    elif x < 0:
        sn += x
if sn != 0:
    r = sp / abs(sn)  # результат

