# 11.103
arr = [10, -4, 12, 56, -4, -89, 7, 23, -15]
sign_changes = 0
if len(arr) > 1:
    prev_sign = 1 if arr[0] > 0 else -1
    for i in range(1, len(arr)):
        curr_sign = 1 if arr[i] > 0 else -1
        if curr_sign != prev_sign:
            sign_changes += 1
            prev_sign = curr_sign

# 11.104
arr = [1, 2, 3, 3, 3, 4, 5, 5, 6, 7, 7, 7, 7, 8, 9, 9, 10, 10, 10, 10]
equal_seq_cnt = 0
i = 0
while i < len(arr):
    j = i
    while j < len(arr) and arr[j] == arr[i]:
        j += 1
    if j - i > 1:  # если подряд идущих одинаковых больше 1
        equal_seq_cnt += (j - i)
    i = j

# Количество различных чисел
unique_cnt = 1  # первый элемент уже учтен
for i in range(1, len(arr)):
    if arr[i] != arr[i-1]:
        unique_cnt += 1

