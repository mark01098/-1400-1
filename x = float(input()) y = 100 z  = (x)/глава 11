# 11.62
houses = [10, 15, 12, 20, 18, 25, 14, 30]  # жильцов в каждом доме
even = 0  # четная сторона
odd = 0   # нечетная сторона
for i in range(len(houses)):
    if (i + 1) % 2 == 0:  # i+1 - номер дома
        even += houses[i]
    else:
        odd += houses[i]
if even > odd:
    side = "четная"
elif odd > even:
    side = "нечетная"
else:
    side = "одинаково"

