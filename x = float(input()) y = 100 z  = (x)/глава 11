# 11.192
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22, 5, 28, 14, 20, 9, 17]

# Находим элементы, которые встречаются ровно два раза
counts = {}
for x in arr:
    if x in counts:
        counts[x] += 1
    else:
        counts[x] = 1

# Ищем элемент, который встречается ровно два раза
duplicate_value = None
for value, count in counts.items():
    if count == 2:
        duplicate_value = value
        break

# 11.195
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22]

# а) номер первого нечетного элемента
first_odd_idx = -1
for i in range(len(arr)):
    if arr[i] % 2 != 0:
        first_odd_idx = i
        break

if first_odd_idx == -1:
    print("Нет нечетных элементов")

# б) номер первого элемента, кратного 13
first_multiple_idx = -1
for i in range(len(arr)):
    if arr[i] % 13 == 0:
        first_multiple_idx = i
        break

if first_multiple_idx == -1:
    print("Нет элементов, кратных 13")

# 11.196
arr = [15.5, 5.2, -25.8, 8.1, 30.4, -12.7, 18.3, 7.9, -22.6]

# Проверяем, есть ли отрицательные числа
has_negative = False
for x in arr:
    if x < 0:
        has_negative = True
        break

if has_negative:
    # а) номер первого отрицательного и все следующие элементы
    first_neg_idx = -1
    for i in range(len(arr)):
        if arr[i] < 0:
            first_neg_idx = i
            break
    
    if first_neg_idx != -1:
        following_elements = arr[first_neg_idx+1:]
    
    # б) номер последнего отрицательного и все элементы слева от него
    last_neg_idx = -1
    for i in range(len(arr)-1, -1, -1):
        if arr[i] < 0:
            last_neg_idx = i
            break
    
    if last_neg_idx != -1:
        left_elements = arr[:last_neg_idx]

