# 11.140
feb_temp = [-5, -3, -7, -2, -8, -4, -6, -1, -9, -3]
cold1 = feb_temp[0]
cold2 = feb_temp[1]
if cold2 < cold1:  # чем меньше температура - тем холоднее
    cold1, cold2 = cold2, cold1

for i in range(2, len(feb_temp)):
    if feb_temp[i] < cold1:
        cold2 = cold1
        cold1 = feb_temp[i]
    elif feb_temp[i] < cold2:
        cold2 = feb_temp[i]

