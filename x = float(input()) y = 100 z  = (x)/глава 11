# 11.216
print("11.216")
areas = [1200, 1500, 800, 2000, 1700, 1900, 1600, 2100, 1400, 1300,
         2200, 1800, 2000, 1600, 1900, 1700, 2100, 1500, 2300, 1400]

harvests = [4800, 6750, 3200, 9000, 7650, 8550, 7200, 9450, 6300, 5850,
            9900, 8100, 9000, 7200, 8550, 7650, 9450, 6750, 10350, 6300]

district_yields = []
for i in range(len(areas)):
    if areas[i] > 0:
        yield_per_hectare = harvests[i] / areas[i]
    else:
        yield_per_hectare = 0
    district_yields.append(yield_per_hectare)

total_area = 0
for x in areas:
    total_area += x

total_harvest = 0
for x in harvests:
    total_harvest += x

if total_area > 0:
    overall_yield = total_harvest / total_area
else:
    overall_yield = 0

