new_arr = [0] * (len(arr) + 1)
found = False
j = 0

for i in range(len(arr)):
    new_arr[j] = arr[i]
    j += 1
    if not found and arr[i] < 0:
        new_arr[j] = insert_value
        j += 1
        found = True

