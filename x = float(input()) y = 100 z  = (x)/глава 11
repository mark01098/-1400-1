    for i in range(even_idx, len(arr)-1):
        arr[i] = arr[i+1]
    arr[-1] = 0

# 11.154
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22]

# Находим индексы максимума и минимума
max_idx = 0
min_idx = 0
for i in range(1, len(arr)):
    if arr[i] > arr[max_idx]:
        max_idx = i
    if arr[i] < arr[min_idx]:
        min_idx = i

# Удаляем максимальный (сначала тот, который имеет больший индекс)
if max_idx > min_idx:
    # Удаляем максимум
    for i in range(max_idx, len(arr)-1):
        arr[i] = arr[i+1]
    arr[-1] = 0
    # Корректируем индекс минимума, если нужно
    if min_idx > max_idx:
        min_idx -= 1
    # Удаляем минимум
    for i in range(min_idx, len(arr)-2):
        arr[i] = arr[i+1]
    arr[-2] = 0
else:
    # Удаляем минимум
    for i in range(min_idx, len(arr)-1):
        arr[i] = arr[i+1]
    arr[-1] = 0
    # Корректируем индекс максимума
    if max_idx > min_idx:
        max_idx -= 1
    # Удаляем максимум
    for i in range(max_idx, len(arr)-2):
        arr[i] = arr[i+1]
    arr[-2] = 0

