    arr[i] = 0

# в) удалить элементы с n1-го по n2-й
arr = list(range(1, 16))  # [1, 2, 3, ..., 15]
n1 = 3
n2 = 8
# Элементы с индексами n1-1 по n2-1
del_count = n2 - n1 + 1
for i in range(n2, len(arr)):
    arr[i-del_count] = arr[i]

# Заполняем нулями
for i in range(len(arr)-del_count, len(arr)):
    arr[i] = 0

# 11.157
arr = [15, 4, 25, 8, 30, 12, 18, 6, 22, 10, 28, 14, 20, 9, 17]

# а) удалить все четные элементы, стоящие на нечетных местах
write_idx = 0
for i in range(len(arr)):
    # i - индекс (начинается с 0), значит i+1 - номер позиции
    if not ((i+1) % 2 != 0 and arr[i] % 2 == 0):  # НЕ (нечетная позиция И четный элемент)
        arr[write_idx] = arr[i]
        write_idx += 1

