# Находим индекс последнего четного элемента
last_even_idx = -1
for i in range(len(arr)):
    if arr[i] % 2 == 0:
        last_even_idx = i

if last_even_idx != -1:
    j = 0
    for i in range(len(arr)):
        if i == last_even_idx:
            new_arr[j] = insert_value
            j += 1
        new_arr[j] = arr[i]
        j += 1
else:
    # Если четных элементов нет, вставляем в начало
    new_arr[0] = insert_value
    for i in range(len(arr)):
        new_arr[i+1] = arr[i]

# 11.161
# Предположим, что массивы отсортированы по алфавиту названий вершин
mountains = ['Аннапурна', 'Дхаулагири', 'Канченджанга', 'Лхоцзе', 'Макалу', 
             'Манаслу', 'Нанга Парбат', 'Эверест']
heights = [8091, 8167, 8586, 8516, 8485, 8163, 8126, 8848]

# Новая вершина "Чогори" (К2) с высотой 8611
new_mountain = 'Чогори'
new_height = 8611

# Определяем позицию для вставки (по алфавиту)
# Для простоты примера зададим позицию вручную
insert_pos = 7  # например, после "Нанга Парбат"

# Создаем новые массивы
new_mountains = [0] * (len(mountains) + 1)
new_heights = [0] * (len(heights) + 1)

# Копируем элементы до позиции вставки
for i in range(insert_pos):
    new_mountains[i] = mountains[i]
    new_heights[i] = heights[i]

# Вставляем новый элемент
new_mountains[insert_pos] = new_mountain
new_heights[insert_pos] = new_height

# Копируем оставшиеся элементы
for i in range(insert_pos, len(mountains)):
    new_mountains[i+1] = mountains[i]
    new_heights[i+1] = heights[i]

# 11.162
heights = [185, 182, 180, 178, 177, 175, 174, 173, 172, 170, 169, 168, 167, 165, 163]

# 1) Известен порядковый номер нового ученика
new_height1 = 176
position1 = 4  # например, на 4-е место (индекс 3)

new_heights1 = [0] * (len(heights) + 1)
for i in range(position1 - 1):
    new_heights1[i] = heights[i]
new_heights1[position1 - 1] = new_height1
for i in range(position1 - 1, len(heights)):
    new_heights1[i+1] = heights[i]

# 2) Известен рост нового ученика (массив отсортирован по убыванию)
new_height2 = 176
new_heights2 = [0] * (len(heights) + 1)

# Находим позицию для вставки
insert_idx = 0
while insert_idx < len(heights) and heights[insert_idx] > new_height2:
    insert_idx += 1

# Вставляем на найденную позицию
for i in range(insert_idx):
    new_heights2[i] = heights[i]
new_heights2[insert_idx] = new_height2
for i in range(insert_idx, len(heights)):
    new_heights2[i+1] = heights[i]

# 11.164
arr = [15, -5, 25, 8, -30, 12, 18, -7, 22, 10, -3, 28, 14, -2]

# а) вставить число 0 перед всеми элементами, кратными a=5
insert_value_a = 0
a = 5
count_multiples = 0

# Считаем, сколько элементов кратны a
for x in arr:
    if x % a == 0:
        count_multiples += 1

# Максимальный размер исходного массива: len(arr) + count_multiples
new_arr_a = [0] * (len(arr) + count_multiples)
j = 0

for i in range(len(arr)):
    if arr[i] % a == 0:
        new_arr_a[j] = insert_value_a
        j += 1
    new_arr_a[j] = arr[i]
    j += 1

# б) вставить число 100 после всех отрицательных элементов
insert_value_b = 100
count_negative = 0

for x in arr:
    if x < 0:
        count_negative += 1

# Максимальный размер исходного массива: len(arr) + count_negative
new_arr_b = [0] * (len(arr) + count_negative)
j = 0

for i in range(len(arr)):
    new_arr_b[j] = arr[i]
    j += 1
    if arr[i] < 0:
        new_arr_b[j] = insert_value_b
        j += 1

# 11.165
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22, 3, 28, 14, 2]
n = 20
m = 10
count_greater_n = 0
count_less_m = 0

for x in arr:
    if x > n:
        count_greater_n += 1
    if x < m:
        count_less_m += 1

# Максимальный размер исходного массива: len(arr) + count_greater_n + count_less_m
new_arr = [0] * (len(arr) + count_greater_n + count_less_m)
j = 0

for i in range(len(arr)):
    # Вставляем n перед элементами, большими n
    if arr[i] > n:
        new_arr[j] = n
        j += 1
    
    new_arr[j] = arr[i]
    j += 1
    
    # Вставляем m после элементов, меньших m
    if arr[i] < m:
        new_arr[j] = m
        j += 1

# 11.166
heights = [185, 182, 180, 178, 177, 175, 174, 173, 172, 170, 
           169, 168, 167, 165, 163, 162, 161, 160, 159, 158, 
           157, 156, 155, 154, 153]

# 1) Известны порядковые номера новых учеников
new_heights1 = [176, 165]  # роста новых учеников
positions = [4, 15]  # порядковые номера (начинаются с 1)

# Сортируем позиции по убыванию для правильной вставки
positions.sort(reverse=True)
new_arr1 = heights.copy()

for pos, height in zip(positions, new_heights1[::-1]):
    new_arr1.insert(pos - 1, height)

# 2) Известны значения роста новых учеников (массив отсортирован по убыванию)
new_heights2 = [176, 165]
new_arr2 = heights.copy()

for height in new_heights2:
    # Находим позицию для вставки
    insert_idx = 0
    while insert_idx < len(new_arr2) and new_arr2[insert_idx] > height:
        insert_idx += 1
    new_arr2.insert(insert_idx, height)

# 11.167
arr = [15, 25, 8, 30, 5, 12, 18, 7, 52, 10, 35, 14, 2]
a = 999  # число для вставки
count_fives = 0

# Проверяем, содержит ли число цифру 5
def has_digit_five(num):
    num_str = str(abs(num))
    return '5' in num_str

# Считаем, сколько элементов содержат цифру 5
for x in arr:
    if has_digit_five(x):
        count_fives += 1

# Максимальный размер исходного массива: len(arr) + count_fives
new_arr = [0] * (len(arr) + count_fives)
j = 0

for i in range(len(arr)):
    new_arr[j] = arr[i]
    j += 1
    if has_digit_five(arr[i]):
        new_arr[j] = a
        j += 1

# 11.168
arr = [15, -5, -3, 8, 12, -7, -2, 22, 25, -10]
n = 0  # число для вставки

# Считаем, сколько пар соседних элементов имеют одинаковый знак
count_pairs = 0
for i in range(len(arr) - 1):
    if (arr[i] > 0 and arr[i+1] > 0) or (arr[i] < 0 and arr[i+1] < 0):
        count_pairs += 1

# Максимальный размер исходного массива: len(arr) + count_pairs
new_arr = [0] * (len(arr) + count_pairs)
j = 0

new_arr[j] = arr[0]
j += 1

for i in range(1, len(arr)):
    # Проверяем, имеют ли текущий и предыдущий элементы одинаковый знак
    if (arr[i-1] > 0 and arr[i] > 0) or (arr[i-1] < 0 and arr[i] < 0):
        new_arr[j] = n
        j += 1
    
    new_arr[j] = arr[i]
    j += 1

# 11.169
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22]
first = arr[0]

# Сдвигаем все элементы влево на 1 позицию
for i in range(len(arr) - 1):
    arr[i] = arr[i+1]

# Помещаем первый элемент в конец
arr[-1] = first

# 11.170
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22]
k = 4  # например, на место 4-го элемента

if 1 < k <= len(arr):
    first = arr[0]
    # Сдвигаем элементы с 1-го по (k-1)-й влево
    for i in range(k-1):
        arr[i] = arr[i+1]
    # Помещаем первый элемент на k-ю позицию
    arr[k-1] = first

# 11.172
results = [120, 118, 122, 115, 125, 119, 121, 117, 123, 116]  # время в секундах
# Массив должен быть упорядочен по ухудшению результата (увеличению времени)
# Если первый элемент не самый худший, нужно упорядочить

# Находим правильную позицию для первого элемента
first = results[0]
i = 1
while i < len(results) and results[i] < first:
    i += 1

# Сдвигаем элементы влево
for j in range(i-1):
    results[j] = results[j+1]

# Вставляем первый элемент на найденную позицию
results[i-1] = first

# 11.173
arr = [15, 5, 25, 8, 30, 12, 18, 7, 22]
last = arr[-1]

# Сдвигаем все элементы вправо на 1 позицию
for i in range(len(arr)-1, 0, -1):
    arr[i] = arr[i-1]

# Помещаем последний элемент в начало
arr[0] = last

# 11.174
