# 11.221
goals_scored = [85, 78, 92, 88, 95, 82, 90, 87, 93, 80, 91, 89, 84, 96, 83]
goals_conceded = [78, 82, 85, 90, 88, 79, 86, 84, 89, 81, 87, 83, 80, 92, 85]

# Способ 1: без дополнительного массива
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        result = "выигрыш"
    else:
        result = "проигрыш"
    print(f"Игра {i+1}: {result} ({goals_scored[i]}:{goals_conceded[i]})")

# Способ 2: с дополнительным массивом
results = [''] * len(goals_scored)
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        results[i] = "выигрыш"
    else:
        results[i] = "проигрыш"

# 11.223
goals_scored = [3, 2, 1, 4, 2, 3, 1, 2, 3, 4, 2, 1, 3, 2, 4, 1, 2, 3, 2, 1]
goals_conceded = [2, 3, 1, 2, 4, 3, 2, 1, 3, 2, 4, 1, 2, 3, 2, 1, 4, 3, 2, 1]

# а) словесный результат каждой игры
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        result = "выигрыш"
    elif goals_scored[i] == goals_conceded[i]:
        result = "ничья"
    else:
        result = "проигрыш"
    print(f"Игра {i+1}: {result}")

# б) количество выигрышей
wins = 0
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        wins += 1

# в) количество выигрышей и проигрышей
wins = 0
losses = 0
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        wins += 1
    elif goals_scored[i] < goals_conceded[i]:
        losses += 1

# г) количество выигрышей, ничьих и проигрышей
wins = 0
draws = 0
losses = 0
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        wins += 1
    elif goals_scored[i] == goals_conceded[i]:
        draws += 1
    else:
        losses += 1

# д) игры с разностью мячей >= 3
big_wins = 0
for i in range(len(goals_scored)):
    if abs(goals_scored[i] - goals_conceded[i]) >= 3:
        big_wins += 1

# е) общее число очков
total_points = 0
for i in range(len(goals_scored)):
    if goals_scored[i] > goals_conceded[i]:
        total_points += 3
    elif goals_scored[i] == goals_conceded[i]:
        total_points += 1

