arr = [15, 5, 25, 8, 30, 12, 30, 7, 5, 22, 5, 30]

# а) номера всех элементов с минимальным значением
min_val = arr[0]
min_indices = [0]
for i in range(1, len(arr)):
    if arr[i] < min_val:
        min_val = arr[i]
        min_indices = [i]
    elif arr[i] == min_val:
        min_indices.append(i)

# б) номера всех элементов с максимальным значением
max_val = arr[0]
max_indices = [0]
for i in range(1, len(arr)):
    if arr[i] > max_val:
        max_val = arr[i]
        max_indices = [i]
    elif arr[i] == max_val:
        max_indices.append(i)

# 11.125
arr = [15.5, 5.2, 25.8, 8.1, 30.4, 12.7, 18.3, 7.9, 22.6]

max_val = arr[0]
min_val = arr[0]
for x in arr:
    if x > max_val:
        max_val = x
    if x < min_val:
        min_val = x

# а) верно ли, что максимальный превышает минимальный не более чем на 25
res_a = (max_val - min_val) <= 25

# б) верно ли, что минимальный меньше максимального более чем в 2 раза
res_b = min_val * 2 < max_val

